		return 1;
	}

	new State=GetPlayerState(playerid);
	
    if(PlayerInfo[playerid][pFUN] == 0)
    {
		if(State!=PLAYER_STATE_DRIVER)
    	{
		if(GetPlayerVirtualWorld(playerid) == 0)
		{
			if( Teleporting[playerid] == true )
			{
			RemoveTeleportMessage(playerid);
			Teleporting[playerid] = false;
			}

		    new Float:x, Float:y, Float:z;
		    GetPlayerPos(playerid, x, y, z);

			new name[MAX_PLAYER_NAME], string[128];
			GetPlayerName(playerid, name, sizeof(name));
			format(string, sizeof(string), "{FF9900}INFO: %s(%i) \n{FF0000}panaudojo /CAR!", name, playerid );
			SendTeleportMessageToAdmins(string);
			SendClientMessage(playerid, COLOR_INFO, "Teleport: /SF /LV /LS");

			TeleportMessage[playerid] = Create3DTextLabel(string, COLOR_INFO, x, y, z, 75.0, 0, 0);
			Teleporting[playerid] = true;

		    teleport_timer[playerid] = SetTimerEx("RemoveTeleportMessage",10000,0,"i", playerid);
		}
		
	    if(PlayerInfo[playerid][pTeleporting]==1) return AlreadyTeleporting(playerid);
	    PlayerInfo[playerid][pTeleporting]=1;
	    ks1(playerid);
	    ResetKillStreak(playerid);
	    if(PlayerInfo[playerid][pVIP]<VIP_TELE){
		TeleportCar1Timer[playerid] = SetTimerEx("ZaidejaNuteleportuotiSGARAGE",5000,0,"i", playerid);
	    ShowTeleportMessage(playerid);
	    }else{
	    ZaidejaNuteleportuotiSGARAGE(playerid);}
		
		}
		else
		{
    		switch (PlayerInfo[playerid][pKalba])
        	{
        	    case 0:
        	    {
        	        SendClientMessage(playerid, COLOR_ERROR, "[INFO] Sorry, you cannot use menu while in vehicle.");
        	    }
        	    case 1:
        	    {
        	        SendClientMessage(playerid, COLOR_ERROR, "[INFO] Apgailestaujame, bet masinoje naudotis meniu draudþiama.");
        	    }
    		}
    	}
    	return 1;
	}

	if(State!=PLAYER_STATE_DRIVER)
    {
    	ShowModelSelectionMenu(playerid, vehiclelist, "Select Vehicle");
    	return 1;
	}
	else
	{
    	switch (PlayerInfo[playerid][pKalba])
        {
            case 0:
            {
                SendClientMessage(playerid, COLOR_ERROR, "[INFO] Sorry, you cannot use menu while in vehicle.");
            }
            case 1:
            {
                SendClientMessage(playerid, COLOR_ERROR, "[INFO] Apgailestaujame, bet masinoje naudotis meniu draudþiama.");
            }
    	}
    }
	return true;
}

