		}
	}

	new x = 0;
	while(x!=MAX_PLAYERS)
	{
    if( IsPlayerConnected(x) &&	GetPlayerState(x) == PLAYER_STATE_SPECTATING &&
	AccInfo[x][SpecID] == playerid && AccInfo[x][SpecType] == ADMIN_SPEC_TYPE_PLAYER)
	{
    SetPlayerInterior(x,newinteriorid);
	}
	x++;
	}

	if(PlayerInfo[playerid][pKova] == 1 || PlayerInfo[playerid][pRKova] == 1 || PlayerInfo[playerid][pMinigun] == 1 || PlayerInfo[playerid][pDisco] == 1 || IsPlayerInWarGames[playerid] == true || ZaidejasSurvival[playerid] == true || InCMEvent[playerid] == true ||
