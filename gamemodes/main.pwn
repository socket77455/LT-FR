public MinigameUpdate( )
{
	if( Iter_Count(_Minigamer ) < 1 ) return EndMinigame( );

	new str[128], Float:playerx, Float:playery, Float:playerz[ALL_PLAYERS];
	foreach(_Minigamer, i )
	{
		GetPlayerPos( i, playerx, playery, playerz[i] );
		if( playerz[i] < 2.0 ) //Checks if player is in the water
		{
			format( str, sizeof( str ), "[/WET] %s(%i) iðkrito ið Wet mini þaidimo!", PlayerNick( i ),i);
			SendClientMessageToAll( COLOR_INFO, str );
			Iter_Remove(_Minigamer, i );
			Minigamer_{ i } = false;
			Paliktiviska( i );
		}
	}
	if( Iter_Count(_Minigamer ) < 2 )
	{
 		foreach(_Minigamer, i ) MinigameWinner( i );
	}
 	new objectid, Float:ObjectX, Float:ObjectY, Float:ObjectZ;

    if(!VIEW_FROM_ABOVE)
    {
	 	foreach(_Objects, i )
		{
			if( isodd( random( 10 ) ) )
			{
		 	    GetDynamicObjectPos( Objects_[0][i], ObjectX, ObjectY, ObjectZ );
				MoveDynamicObject( Objects_[0][i], ObjectX, ObjectY, ObjectZ -1.5, 0.2 );
	   			MoveDynamicObject( Objects_[1][i], ObjectX, ObjectY, ObjectZ -1.5, 0.2 );
			}
			else
			{
				GetDynamicObjectPos( Objects_[0][i], ObjectX, ObjectY, ObjectZ );
				MoveDynamicObject( Objects_[0][i], ObjectX, ObjectY, ObjectZ +1.5, 0.2 );
				MoveDynamicObject( Objects_[1][i], ObjectX, ObjectY, ObjectZ +1.5, 0.2 );
			}
		}
	}

 	objectid = Iter_Random(_Objects );
	GetDynamicObjectPos( Objects_[0][objectid], ObjectX, ObjectY, ObjectZ );
	SetTimerEx("SpeedUp", 1000, 0, "ifff", objectid, ObjectX, ObjectY, ObjectZ);
	MoveDynamicObject( Objects_[0][objectid], ObjectX, ObjectY, ObjectZ -5, 1 );
	if(!VIEW_FROM_ABOVE)
	MoveDynamicObject( Objects_[1][objectid], ObjectX, ObjectY, ObjectZ -5, 1 );
    Iter_Remove(_Objects, objectid );
	return 1;
}

