forward PayDay();
public PayDay()
{
	new str[128];
	
	foreach (Player, playerid )
	{
    	if(PlayerInfo[playerid][pLogged] == 0) return NotLogged(playerid);
		if(IsPlayerAFK(playerid)) continue;

		new PayDay_Amount,expGain;
    	PayDay_Amount = Player[playerid][Level]*4+Player[playerid][Level]*10;
    	expGain = 500;
    	GivePlayerPCash(playerid,PayDay_Amount);
    	GivePlayerExp(playerid,expGain);
    	PlayerPlaySound(playerid, 1057, 0.0, 0.0, 0.0);

		switch (PlayerInfo[playerid][pKalba])
    	{
    		case 0:
    		{
				SendClientMessage(playerid, COLOR_ERROR, "==== [ www.freeroam.lt Freeroam players status: www.freeroam.lt ] ====");
    			format(str, 128, "==== [ PayDay $%d ] === [ Exp + %d / %d ] ====",PayDay_Amount,expGain,Player[playerid][Exp]);
				SendClientMessage(playerid, COLOR_ERROR, str);
				SendClientMessage(playerid, COLOR_ERROR, "==== [ This server's website: www.freeroam.lt ] ====");
    		}
    		case 1:
    		{
    			SendClientMessage(playerid, COLOR_ERROR, "==== [ Lithuanian Freeroam þaidëjø statusai: {1AFF00}www.freeroam.lt{FF0000} ] ====");
    			format(str, 128, "==== [ Uþmokestis $%d ] === [ EXP + %d / %d ] ====",PayDay_Amount,expGain,Player[playerid][Exp]);
				SendClientMessage(playerid, COLOR_INFO, str);
				SendClientMessage(playerid, COLOR_ERROR, "==== [ Ðio serverio tinklalapis: {1AFF00}www.freeroam.lt{FF0000} ] ====");
    		}
		}
	}
    return 1;
}

forward UnArmPlayer();
public UnArmPlayer()
//stock UnArmPlayer(playerid)
{
	foreach (Player, playerid )
	{
	if(GetPlayerInterior(playerid) != 0)
	{
		if(GetPlayerVirtualWorld(playerid) == 0)
		{
			if(GetPlayerWeapon(playerid) >= 2)
			{
				// Bankai
				if(IsPlayerInRangeOfPoint(playerid, 20.0, -22.4887,-54.9330,1003.5469)) // Bankas LV
				{
					SetPlayerArmedWeapon(playerid, 0);
				}
				if(IsPlayerInRangeOfPoint(playerid, 30.0, 206.9305,-7.0260,1001.2109)) // Bankas SF
				{
					SetPlayerArmedWeapon(playerid, 0);
				}
				if(IsPlayerInRangeOfPoint(playerid, 25.0, -26.8282,-90.0157,1003.5469)) // Bankas LS
				{
					SetPlayerArmedWeapon(playerid, 0);
				}
				
				// Ammunation
				if(IsUsingAmmunation[playerid] == 0)
				{
					if(IsPlayerInRangeOfPoint(playerid, 15.5, 288.0167,-34.7613,1001.5156)) // Ammunation SF
					{
						SetPlayerArmedWeapon(playerid, 0);
					}
					if(IsPlayerInRangeOfPoint(playerid, 30.0, 289.9954,-80.8648,1001.5156)) // Ammunation LV
					{
						SetPlayerArmedWeapon(playerid, 0);
					}
					if(IsPlayerInRangeOfPoint(playerid, 15.0, 296.3623,-104.8923,1001.5156)) // Ammunation LS
					{
						SetPlayerArmedWeapon(playerid, 0);
					}
					if(IsPlayerInRangeOfPoint(playerid, 15.0, 317.7813,-160.0079,999.5938)) // Ammunation LV Desert Special
					{
						SetPlayerArmedWeapon(playerid, 0);
					}
				}
			}
		}
	}
	}
    return 1;
}
