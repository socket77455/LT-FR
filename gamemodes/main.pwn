COMMAND:abuse(playerid,params[])
{
	if(PlayerInfo[playerid][pLogged] == 0) return NotLogged(playerid);
	if(Player[playerid][pAdmin] < 1 && !PlayerInfo [ playerid ] [ pMod ]) return OnlyAdmins(playerid);
	
    if ( isnull ( params ) )return SendClientMessage(playerid, COLOR_INFO, "USAGE: /abuse <playerid>");

    new slapid = strval ( params );
    if(slapid == INVALID_PLAYER_ID)
		return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Þaidëjas nerastas!");
 	if(!IsPlayerConnected(slapid))
 		return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Þaidëjas neprisijungæs!");
	if(slapid == playerid)
	    return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You are not able to teleban yourself!", "KLAIDA: Sau uþdrausti negalima!");
    else
    {
        new Float:X, Float:Y, Float:Z,AdminName[MAX_PLAYER_NAME],SlapName[MAX_PLAYER_NAME],AbuseMessage[128];
        GetPlayerPos(slapid, X, Y, Z);
        SetPlayerPos(slapid, X, Y, Z+24);
        GetPlayerName(slapid, SlapName, 24);
        GetPlayerName(playerid, AdminName, 24);
        format(AbuseMessage, sizeof(AbuseMessage), "%s(%i) buvo uþdrausta teleportuotis. (30 minuèiø) (Prieþastis: Tele Abuse) (Administratorius: %s)!", SlapName, slapid, AdminName);
        SendClientMessageToAll(COLOR_ERROR, AbuseMessage);
        print ( AbuseMessage );

       	Player[slapid][pTeleAbuse] = 1800;
		Player[slapid][pDeaths] ++;
		
		if ( Player [ slapid ] [ pGang ] != 0 )
		{
 			mysql_format(g_SQL, query, sizeof(query), "UPDATE gangs SET deaths = deaths + 1 WHERE ID = '%d'", Player[slapid][pGang]);
			mysql_tquery(g_SQL, query);
		}
		
		SetPlayerHealth(slapid, 0);
		ShowPlayerRules(slapid);
		PlayerInfo[slapid][pPlayerRules] = 0;

  	}
    return 1;
}

