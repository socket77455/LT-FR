public OnPlayerDisconnect(playerid, reason)
{
    g_MysqlRaceCheck[playerid]++;
    SavePlayerWeapons(playerid);

	SetPlayerColor(playerid, playerColors[playerid]);
	SetPlayerArmour(playerid, 0);

	DiedInPVP[ playerid ] = 0;
	PlayerInfo[playerid][PVPBO] = 0;

	KillTimer(spectate_timer[playerid]);
	KillTimer(lab_timer[playerid]);

    if(PlayerInfo[playerid][pBriefcase] == 1)
    {
        BriefcaseDestroyed();
        DisablePlayerCheckpoint(playerid);
        PlayerInfo[playerid][pBriefcase] = 0;
        KillTimer(briefcase_timer[playerid]);
    }

	if(PersonalVehicle[playerid] != INVALID_VEHICLE_ID)
	{
		DestroyVehicle(PersonalVehicle[playerid]);
		PersonalVehicle[playerid] = INVALID_VEHICLE_ID;
	}

	if(Player[playerid][pGang] >= 1)
	{
		cmd_gang(playerid, "close");
	}

 	DestroyProgressBar(zonebar[playerid]);

    new name[MAX_PLAYER_NAME];
	GetPlayerName(playerid,name,24);
	new string[128];
	foreach (Player, i )
	{
		if(PlayerInfo[i][pKalba] == 0)
		{
			switch (reason)
			{
				case 0: format(string, 128, "[LEFT] %s(%d) has left the server. (Timeout)", name, playerid);
				case 1: format(string, 128, "[LEFT] %s(%d) has left the server. (Leaving)", name, playerid);
				case 2: format(string, 128, "[LEFT] %s(%d) has left the server. (Kicked)", name, playerid);
			}
			SendClientMessage(i,COLOR_LIGHTBLUE, string);
		}
		else
		{
			switch (reason)
			{
				case 0: format(string, 128, "[Iðeina] %s(%d) paliko serverá. (Klaida)", name, playerid);
				case 1: format(string, 128, "[Iðeina] %s(%d) paliko serverá. (Iðeina)", name, playerid);
				case 2: format(string, 128, "[Iðeina] %s(%d) paliko serverá. (Iðmestas)", name, playerid);
			}
			SendClientMessage(i,COLOR_LIGHTBLUE, string);
		}
	}

	if(!IsPlayerNPC(playerid))
	{
		ServerioZaidejai--;
		SendDeathMessage(INVALID_PLAYER_ID, playerid, 201);
	}

    ResetStats(playerid);

    DestroyPlayerObject(playerid, disco1[playerid]);
    DestroyPlayerObject(playerid, disco2[playerid]);
    DestroyPlayerObject(playerid, disco3[playerid]);
    DestroyPlayerObject(playerid, disco4[playerid]);
    PlayerInfo[playerid][pDisco] = 0;
    PlayerInfo[playerid][pPlayerRules] = 0;
    DestroyMessageTextDraw(playerid);

	GPS_Spawned[playerid] = 0;

    KillTimer(speed_detection[playerid]);
