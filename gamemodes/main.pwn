	SetTimer ("TDScore", 1000, 1);
}

public TimerMove ()
{
	new rand;
	new Hay;
	new Move = -1;
	new xq, yq, zq;
	new Float:x2, Float:y2, Float:z2;
	new Timez;
	new Float:HeySpeed;

	rand = random (HAY_B);
	Hay = Hays[rand];
	if(IsDynamicObjectMoving(Hay))
	{
		SetTimer ("TimerMove", 200, 0);
		return 1;
	}
	Move = random (6);
	GetDynamicObjectPos (Hay, x2, y2, z2);
	xq = floatround (x2/-4.0);
	yq = floatround (y2/-4.0);
	zq = floatround (z2/3.0)-1;
	if ((Move == 0)  && (xq != 0) && (Matrix[xq-1][yq][zq] == 0))
	{
		Timez = 4000 - HeySpeed_xy * zq;
		HeySpeed = HeySpeed_FACTOR / float (Timez);
		SetTimerEx ("FinishTimer", Timez, 0, "iiii", rand, xq, yq, zq);
		xq = xq - 1;
		Matrix[xq][yq][zq] = 1;
		MoveDynamicObject (Hay, x2+4.0, y2, z2, HeySpeed);
	}

	else if ((Move == 1) && (xq != HAY_X-1) && (Matrix[xq+1][yq][zq] == 0))
	{
		Timez = 4000 - HeySpeed_xy * zq;
		HeySpeed = HeySpeed_FACTOR / float (Timez);
		SetTimerEx ("FinishTimer", Timez, 0, "iiii", rand, xq, yq, zq);
		xq = xq + 1;
		Matrix[xq][yq][zq] = 1;
		MoveDynamicObject (Hay, x2-4.0, y2, z2, HeySpeed);
	}

	else if ((Move == 2) && (yq != 0) && (Matrix[xq][yq-1][zq] == 0))
	{
		Timez = 4000 - HeySpeed_xy * zq;
		HeySpeed = HeySpeed_FACTOR / float (Timez);
		SetTimerEx ("FinishTimer", Timez, 0, "iiii", rand, xq, yq, zq);
		yq = yq - 1;
		Matrix[xq][yq][zq] = 1;
		MoveDynamicObject (Hay, x2, y2+4.0, z2, HeySpeed);
	}


	else if ((Move == 3) && (yq != HAY_Y-1) && (Matrix[xq][yq+1][zq] == 0))
	{
		Timez = 4000 - HeySpeed_xy * zq;
		HeySpeed = HeySpeed_FACTOR / float (Timez);
		SetTimerEx ("FinishTimer", Timez, 0, "iiii", rand, xq, yq, zq);
		yq = yq + 1;
		Matrix[xq][yq][zq] = 1;
		MoveDynamicObject (Hay, x2, y2-4.0, z2, HeySpeed);
	}

	else if ((Move == 4) && (zq != 0) && (Matrix[xq][yq][zq-1] == 0))
	{
		Timez = 3000 - HeySpeed_z * zq;
		HeySpeed = HeySpeed_FACTOR / float (Timez);
		SetTimerEx ("FinishTimer", Timez, 0, "iiii", rand, xq, yq, zq);
		zq = zq - 1;
		Matrix[xq][yq][zq] = 1;
		MoveDynamicObject (Hay, x2, y2, z2-3.0, HeySpeed);
	}

	else if ((Move == 5) && (zq != HAY_Z-1) && (Matrix[xq][yq][zq+1] == 0))
	{
		Timez = 3000 - HeySpeed_z * zq;
		HeySpeed = HeySpeed_FACTOR / float (Timez);
		SetTimerEx ("FinishTimer", Timez, 0, "iiii", rand, xq, yq, zq);
		zq = zq + 1;
		Matrix[xq][yq][zq] = 1;
		MoveDynamicObject (Hay, x2, y2, z2+3.0, HeySpeed);
	}
	else if ((Move == 6) && (zq != HAY_Z-1) && (Matrix[xq][yq][zq+1] == 0))
	{
		Timez = 3000 - HeySpeed_z * zq;
		HeySpeed = HeySpeed_FACTOR / float (Timez);
		SetTimerEx ("FinishTimer", Timez, 0, "iiii", rand, xq, yq, zq);
		zq = zq + 1;
		Matrix[xq][yq][zq] = 1;
		MoveDynamicObject (Hay, x2, y2, z2+3.0, HeySpeed);
	}
	SetTimer ("TimerMove", 200, 0);
	return 1;
}

