			else if(Ranking == 3)
			{
				Player[playerid][pLenktynestreciavieta]++;
			}
			
			ZaidejasFinisavoRACE(playerid);
			
			foreach (Player, i )
			{
				if(PlayerInfo[i][pKalba] == 0)
				{
					format(ystring,sizeof(ystring),"~n~~n~~n~~n~~n~~n~~n~~g~ %s ~w~has finished the race, position: ~y~%d",name,Ranking);
					GameTextForPlayer(i,ystring,5000,3);
				}
				else if(PlayerInfo[i][pKalba] == 1)
				{
					format(ystring,sizeof(ystring),"~n~~n~~n~~n~~n~~n~~n~~g~ %s ~w~finisavo lenktynes, vieta: ~y~ %d",name,Ranking);
					GameTextForPlayer(i,ystring,5000,3);
				}
			}
			if(Ranking<4)
			{
				new winrar;
				if(Ranking == 1 && Participants == 1) winrar=Pot; // If the player was only participant, give the whole pot.
				else if(Ranking == 1 && Participants == 2) winrar=Pot/6*4; // If only 2 participants, give 4/6ths of the pot.
				else winrar=Pot/6*PrizeMP;  // Otherwise 3/6ths, 2/6ths and 1/6th.

				GivePlayerPCash(playerid,winrar);
				GivePlayerExp(playerid,500);
				
				if(Ranking == 1)
				{
					new str[256];
					format(str, 256, "WINNER!~n~~w~$%d", winrar);
					GameTextForPlayer(playerid, str, 8000, 0);
					MissionPassedSound(playerid);
				}

                if(Ranking != 1)
                {
		            switch (PlayerInfo[playerid][pKalba])
