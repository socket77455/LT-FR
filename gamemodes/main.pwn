forward RemovePickups(playerid);
public RemovePickups(playerid)
{
	if(WeaponTimer[playerid] != -1) KillTimer(WeaponTimer[playerid]);
	WeaponTimer[playerid] = -1;
	for(new a=0; a<14; a++)
	{
	    if(WeaponDrop[playerid][a] != -1)
	    {
	        DestroyPickup(WeaponDrop[playerid][a]);
	        WeaponDrop[playerid][a] = -1;
	 		WeaponData[playerid][a][0] = -1;
	    	WeaponData[playerid][a][1] = -1;
	    	WeaponData[playerid][a][2] = -1;
	    }
	}
	return 1;
}

CheckBot(playerid)
{
		new ConnIP[16];
		GetPlayerIp(playerid,ConnIP,16);
		new compare_IP[16];
		new number_IP = 0;
		foreach(Player, i){
			if(IsPlayerConnected(i)) {
			    GetPlayerIp(i,compare_IP,16);
			    if(!strcmp(compare_IP,ConnIP)) number_IP++;
			}
		}
		if((GetTickCount() - Join_Stamp) < Time_Limit)
		    exceed=1;
		else
		    exceed=0;
	 	if(strcmp(ban_s, ConnIP, false) == 0 && exceed == 1 )
	 	{
	 	    Same_IP++;
	 	    if(Same_IP > SAME_IP_CONNECT)
	 	    {
		   		BlockIpAddress(ConnIP, 0);
	 			Same_IP=0;
	 	    }
	 	}
	 	else
	 	{
	 		Same_IP=0;
		}
		if(number_IP > IP_LIMIT)
		    Kick(playerid);
		GetStampIP(playerid);
		return 1;
}

