public CheckAccountBan(playerid)
{
    new str[128];
	new name[MAX_PLAYER_NAME];
	GetPlayerName(playerid, name, 24);
	
	if(MySQLCheckAccountBanExistance(playerid))
	{
    	switch (PlayerInfo[playerid][pKalba])
        {
            case 0:
            {
	            SendClientMessage(playerid, COLOR_ERROR, "ERROR: You're banned from this server.");
	            SendClientMessage(playerid, COLOR_ERROR, "Want to play here? You will have to buy unban for 1.50 EUR");
	            SendClientMessage(playerid, COLOR_ERROR, "More information and unban appeals: {1AFF00}www.freeroam.lt");
            }
            case 1:
            {
				SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Jûs esate uþblokuotas.");
				SendClientMessage(playerid, COLOR_ERROR, "Norint þaisti ðiame serve turesite nusipirkti UNBAN uþ 1.50 EUR");
				SendClientMessage(playerid, COLOR_ERROR, "Adresas:{1AFF00} www.freeroam.lt");
            }
        }

	    foreach (Player, i )
		{
			if(PlayerInfo[i][pKalba] == 0)
			{
				format(str, 128, "[SERVER] %s is banned.", name);
				SendClientMessage(i, COLOR_ERROR, str);
			}
			else if(PlayerInfo[i][pKalba] == 1)
			{
				format(str, 128, "[SERVER] %s yra uþblokuotas.", name);
				SendClientMessage(i, COLOR_ERROR, str);
			}
		}

		//Kick(playerid);
		SetTimerEx("KickPlayer",800,0,"i", playerid);
		return true;
	}

	return true;
}


