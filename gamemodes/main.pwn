	for (xq=0 ; xq<HAY_X ; xq++)
	{
		for (yq=0 ; yq<HAY_Y ; yq++)
		{
			for (zq=0 ; zq<HAY_Z ; zq++)
			{
				Matrix[xq][yq][zq] = 0;
			}
		}
	}
	for (HeyNumber=0 ; HeyNumber<HAY_B ; HeyNumber++)
	{
		do
		{
			xq = random (HAY_X);
