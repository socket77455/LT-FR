COMMAND:oras(playerid, params[])
{
	if(PlayerInfo[playerid][pLogged] == 0) return NotLogged(playerid);
	if(Player[playerid][pAdmin] < 4) return OnlyAdmins(playerid);
	//Apsaugos
	
	if(strlen(params) < 1) return SendClientMessage(playerid, COLOR_INFO, "USAGE: /weather (0-20)");
