public ServerioInformacija()
{
	new gamemode[50];
	format(gamemode, 50, "%s %s", SYS_VERSION, SYS_DESC);
	mysql_format(g_SQL, query, sizeof(query), "UPDATE `settings` SET `hostname` = '%s', `gamemode` = '%s', `mapname` = '%s' WHERE `id` = 1", ReturnServerStringVar("hostname"), gamemode, SYS_MAP);
    mysql_tquery(g_SQL, query);
    return 1;
}
//==============================================================================
forward TikrintiRekorda();
public TikrintiRekorda()
{
	if (srekordas < ServerioZaidejai)
	{
		srekordas = ServerioZaidejai;
		new date[13];
		new year, month, day;
		getdate(year, month, day);

		format(date, 13, "%d-%d-%d", year, month, day);

		mysql_format(g_SQL, query, sizeof(query), "UPDATE `settings` SET `date` = '%s', `rekordas` = '%d' WHERE `id` = 1", date, ServerioZaidejai);
		mysql_tquery(g_SQL, query);

		printf("NAUJAS Serverio rekordas: %d", srekordas);

		new msg[70];
		format(msg, 70, "[www.freeroam.lt] -- NAUJAS SERVERIO REKORDAS: %d", srekordas);
		SendClientMessageToAll(COLOR_INFO, msg);
    }
	return 1;
}
