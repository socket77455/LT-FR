public OnPlayerWeaponShot(playerid, weaponid, hittype, hitid, Float:fX, Float:fY, Float:fZ)
{
    if(PlayerInfo[playerid][pFunTime] == 0)
    {
		if(weaponid == 4 || weaponid == 7 || weaponid == 14 || weaponid == 15 || weaponid == 17)
		{
		    Kick(playerid);
		}
		
		if(weaponid == 26 || weaponid == 28 )
		{
		    if(GetPlayerVirtualWorld(playerid) != RDM_ZONOS_PASAULIS)
		    {
		        if(GetPlayerVirtualWorld(playerid) != EVENT_PASAULIS)
		        {
	      			Kick(playerid);
				}
		    }
		}
		if(weaponid == 32)
		{
		    if(GetPlayerVirtualWorld(playerid) != GUNGAME_PASAULIS)
		    {
		        if(GetPlayerVirtualWorld(playerid) != EVENT_PASAULIS)
		        {
	      			Kick(playerid);
				}
		    }
		}
		if(weaponid == 38)
		{
		    if(GetPlayerVirtualWorld(playerid) != MINIGUN_ZONOS_PASAULIS)
		    {
		        if(GetPlayerVirtualWorld(playerid) != EVENT_PASAULIS)
		        {
	      			Kick(playerid);
				}
		    }
		}
	}
	
	//Aimbot Check
	if(checkaim == 0)
	{
	    if(weaponid != 38)
	    {
			if(hittype == BULLET_HIT_TYPE_PLAYER && !IsPlayerNPC(hitid))
		 	{
		  		if(!IsAFK{ hitid } && !CoolDown{ playerid })
		        {
		        	new surf = GetPlayerSurfingVehicleID(playerid), tsurf = GetPlayerSurfingVehicleID(playerid);
					if(surf == INVALID_VEHICLE_ID && tsurf == INVALID_VEHICLE_ID && !IsPlayerInAnyVehicle(hitid))
					{
					    new
							Float:targetpkl = NetStats_PacketLossPercent(hitid),
							Float:playerpkl = NetStats_PacketLossPercent(playerid);
					    if(targetpkl < 0.7 && playerpkl < 0.7)
					    {
						    new Float:fOriginX, Float:fOriginY, Float:fOriginZ, Float:fHitPosX, Float:fHitPosY, Float:fHitPosZ;
							GetPlayerLastShotVectors(playerid, fOriginX, fOriginY, fOriginZ, fHitPosX, fHitPosY, fHitPosZ);
							CheckForAimbot(playerid, fHitPosX, fHitPosY, fHitPosZ, hitid);
						}
					}
		        }
			}
		}
	}

	// Rapid Fire
	if(weaponid != 38)//Prevent to detect Minigun
	{
		if((GetTickCount() - RapidTime[playerid]) < 1)//Detect the intervals
	 	{
			RapidShot[playerid]+=1;
	  		}
			else
	        {
	        RapidShot[playerid]=0;
		}

		if(weaponid == 22 || weaponid == 23)//Pistols
	 	{
			if(RapidShot[playerid] > 7)
	  		{
	   			ResetPlayerWeapons(playerid);
	           	Paliktiviska(playerid);
	      		//return 0;
	      	}
		}

	    if(weaponid == 24 || weaponid == 25 )//Deagle and Shotguns
	    {
	        if(RapidShot[playerid] > 3)
	        {
	       		ResetPlayerWeapons(playerid);
	           	Paliktiviska(playerid);
	           	//return 0;
	        }
	    }

	    if(weaponid == 28 || weaponid == 29)//Submachines
	    {
	        if(RapidShot[playerid] > 15)
	        {
	       		ResetPlayerWeapons(playerid);
	           	Paliktiviska(playerid);
	           	//return 0;
	        }
	    }

	    if(weaponid == 30 || weaponid == 31)//Assault rifles
	    {
	        if(RapidShot[playerid] > 15)
	        {
	       		ResetPlayerWeapons(playerid);
	           	Paliktiviska(playerid);
	           	//return 0;
	        }
	    }

	    RapidTime[playerid] = GetTickCount();
    }
    return 1;
}

