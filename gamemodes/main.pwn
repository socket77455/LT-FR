COMMAND:derby(playerid, params[])
{
    #pragma unused params
    if(Player[playerid][pJail] >= 1)
		return YouAreInJail(playerid);

	if(PlayerInfo[playerid][pKova] == 1 || PlayerInfo[playerid][pRKova] == 1 || PlayerInfo[playerid][pMinigun] == 1 || PlayerInfo[playerid][pDisco] == 1 ||
		PlayerInfo[playerid][pDerby] == 1 || PlayerInfo[playerid][pSKY] == 1 || PlayerInfo[playerid][pGunGame] == 1 ||
		PlayerInfo[playerid][pFUN] == 1 || PlayerInfo[playerid][pInPlaneStunt] == 1 ||
		PlayerInfo[playerid][pSTUNT] >= 1 || PlayerInfo[playerid][pSTUNT2] >= 1 ||
		PlayerInfo[playerid][pSTUNT3] >= 1 || PlayerInfo[playerid][pCBUG] == 1 ||
		RaceParticipant[playerid]>0 || dragParticipant[playerid]>0 ||
		MaratonasParticipant[playerid]>0 || PlayerInfo[playerid][PVPstate] > 1 && PlayerInfo[playerid][PVPstate] < 4 ||
		PlayerInfo[playerid][TDMstate] > 0 || PlayerInfo[playerid][pFuristas] == 1  || PlayerInfo[playerid][pInBMXgame] == 1 || PlayerInfo[playerid][pSTUNT4] >= 1 || PlayerInfo[playerid][pSTUNT5] >= 1 || PlayerInfo[playerid][pSTUNT6] >= 1)
			return UsePlayer(playerid);
	if (Derby != DERBY_STARTING)
        return SendClientMessageLang(playerid, COLOR_YELLOW, "You can't join derby at the moment!", "ðiuo metu prisijungti negalima!");

	if (GetPlayerPCash(playerid) < DERBY_FEE)
		return SendClientMessageLang( playerid, COLOR_ERROR, "ERROR: You haven't enough money to join derby!", "KLAIDA: Jums nepakanka pinigø!" );

	new State=GetPlayerState(playerid);
	if(State != PLAYER_STATE_DRIVER)
    {
        GivePlayerPCash ( playerid, -DERBY_FEE );
        
        new randveh = random(sizeof(RandomMonsterTruck));
		new rand = random ( 11 );
        new vid = CreateVehicle ( RandomMonsterTruck[randveh], DerbyCP [ rand ] [ 0 ], DerbyCP [ rand ] [ 1 ], DerbyCP [ rand ] [ 2 ], 0.0, -1, -1, -1 );
        SetPlayerSpecialSkin	( playerid );
        ResetPlayerWeapons		( playerid );
		SetVehicleVirtualWorld 	( vid, DERBY_PASAULIS );
		SetPlayerVirtualWorld   ( playerid, DERBY_PASAULIS );
		LinkVehicleToInterior   ( vid, 15 );
		SetPlayerInterior       ( playerid, 15 );
		PutPlayerInVehicle 		( playerid, vid, 0 );
		SetPlayerTeam          	( playerid, NO_TEAM );
		DerbyVehicle [ vid ] = true;
		PlayerCPS  [ playerid ] = -2;
		//PlayerInfo [ playerid ] [ pDerby ] = 1;
		SetPlayerColor(playerid, playerColors[playerid]);
		
        new msg_lt[128],msg_en[128],pName[MAX_PLAYER_NAME];
        GetPlayerName(playerid, pName, MAX_PLAYER_NAME);
        format(msg_en,128, "[/DERBY] %s(%i) has joined the Derby.", pName, playerid);
        format(msg_lt,128, "[/DERBY] %s(%i) prisijungë á buliø kautynes.", pName, playerid);

		foreach (Player, i )
		{
        	switch (PlayerInfo[i][pKalba])
			{
				case 0: SendClientMessage(i,COLOR_INFO, msg_en);
				case 1: SendClientMessage(i,COLOR_INFO, msg_lt);
			}
		}
		PlayerInfo [ playerid ] [ pDerby ] = 1;
	}
	else
	{
    	switch (PlayerInfo[playerid][pKalba])
    	{
     		case 0: SendClientMessage(playerid, COLOR_ERROR, "[INFO] Sorry, you cannot use this command while in vehicle.");
            case 1: SendClientMessage(playerid, COLOR_ERROR, "[INFO] Apgailestaujame, bet maðinoje naudotis ðia komanda draudþiama.");
    	}
    }
	return 1;
}
