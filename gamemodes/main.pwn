    SendClientMessage(playerid,COLOR_ERROR,"KLAIDA: Komanda skirta tik VIP vartotojams.");
    return 1;
	}

	GivePlayerWeapon(playerid, WEAPON_PARACHUTE,1);
	return true;
}

