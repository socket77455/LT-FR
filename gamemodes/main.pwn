		SetPlayerName(playerid,newnick);
		format(Player[playerid][player_name], 24, "%s", newnick);

		// atnnaujinam namo 3dtext, jei zaidejas turi nama
		if(Player[playerid][HouseID] != -1)
			Update3DText(Player[playerid][HouseID]);
	}
	else
	{
		switch (PlayerInfo[playerid][pKalba])
        {
            case 0:
            {
				format(str, 128, "INFO: Your new account's (\"%s\") nick is already in use, use another one (new nick \"%s\")", newnick, PlayerName);
				SendClientMessage(playerid, COLOR_INFO, str);
            }
            case 1:
            {
				format(str, 128, "INFO: Jûsø naujos saskaitos (\"%s\") vardas jau egzistuoja duombazeje, naudokite kità.", newnick);
				SendClientMessage(playerid, COLOR_INFO, str);
            }
        }
	}
}

IsValidEmailAddress(const email[])
{
    new at_pos = strfind(email, "@", true);
    if(at_pos >= 1)
    {
        new offset = (at_pos + 1), dot_pos = strfind(email, ".", true, offset);
        if(dot_pos > offset)
        {
            return 1;
        }
    }
    return 0;
}
