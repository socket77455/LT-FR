COMMAND:givegun(playerid, params[])
{
	if(PlayerInfo[playerid][pLogged] == 0) return NotLogged(playerid);
	if(Player[playerid][pAdmin] < 3 && !Player [ playerid ] [ pEventMod ]) return OnlyAdmins(playerid);
	
   	//new tmp[256];
   	new lookupid, weapon, wname[64], ammo;

	if (sscanf(params, "iii",lookupid, weapon, ammo))
	return SendClientMessage(playerid, COLOR_INFO, "/givegun <playerid> <gunid> <ammo>");

	if (!IsPlayerConnected(lookupid)) { return SendClientMessage(playerid, COLOR_ERROR, "Player is not connected."); }
	
	if(lookupid == playerid)
	    return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You are not able to use it yourself!", "KLAIDA: Sau negalite!");

    if (ammo > 500)
        return SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Negalite duoti daugiau nei 500 kulkø");

	if (weapon < 1 || weapon > 46)
		return SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Ginklo ID turi bûti tarp 1 ir 46");

    if (weapon == 4 || weapon == 7 || weapon == 14 || weapon == 15 || weapon == 17 || weapon == 36 ||  weapon == 40 || weapon == 44 || weapon == 45)
        return SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: ðio ginklo duoti negalite!");
		
	if(GetPlayerVirtualWorld(lookupid) != EVENT_PASAULIS)
	{
	SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Player is not in the event zone!", "KLAIDA: Þaidëjas nëra event'e!");
	return 1;
	}

	GivePlayerWeapon(lookupid, weapon, ammo);
	GetWeaponName(weapon, wname, 64);

    new msgen[128],msglt[128];
    format(msgen,256, "[EVENT] %s(%d) gave %s(%d) a %s with %d ammo.", GetName(playerid), playerid, GetName(lookupid), lookupid, wname, ammo);
    format(msglt,256, "[EVENT] %s(%d) davë %s(%d) %s su %d ðoviniø.", GetName(playerid), playerid, GetName(lookupid), lookupid, wname, ammo);
	SendClientMessageToAllLang(COLOR_INFO, msgen, msglt);
    return 1;
}

