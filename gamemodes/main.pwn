AddNewReport(senderid, sendername[], reportedid, reportedname[], reason[]) {
	if(strlen(Report[0][Text]) > 0) {
		for(new i = 9; i > 0; i--) {
            Report[i][Text][0] = EOS;
	    	format(Report[i][Text], 128, "%s", Report[i - 1][Text]);
		    Report[i][sID] = Report[i - 1][sID];
		    Report[i][sName][0] = EOS;
		    format(Report[i][sName], 24, "%s", Report[i - 1][sName]);
		    Report[i][rID] = Report[i - 1][rID];
		    Report[i][rName][0] = EOS;
		    format(Report[i][rName], 24, "%s", Report[i - 1][rName]);
		    Report[i][Status][0] = EOS;
		    format(Report[i][Status], 24, "%s", Report[i - 1][Status]);
		    Report[i][rH] = Report[i - 1][rH];
		    Report[i][rM] = Report[i - 1][rM];
		    Report[i][rS] = Report[i - 1][rS];
		}
		format(Report[0][Text], 128, "%s", reason);
	    Report[0][sID] = senderid;
	    format(Report[0][sName], 24, "%s", sendername);
	    Report[0][rID] = reportedid;
	    format(Report[0][rName], 24, "%s", reportedname);
	    format(Report[0][Status], 24, "Neávertintas");
	    gettime(Report[0][rH], Report[0][rM], Report[0][rS]);
	}else {
	    format(Report[0][Text], 128, "%s", reason);
	    Report[0][sID] = senderid;
	    format(Report[0][sName], 24, "%s", sendername);
	    Report[0][rID] = reportedid;
	    format(Report[0][rName], 24, "%s", reportedname);
	    format(Report[0][Status], 24, "Neávertintas");
	    gettime(Report[0][rH], Report[0][rM], Report[0][rS]);
	}
}

COMMAND:report(playerid, params[])
{
	if(PlayerInfo[playerid][pApsauga] == 1) return SQLapsauga(playerid);
	
	new rsn[128];
	new str[128];
	new giveplayerid;
	new giveplayer [ 24 ];
	
	if (strlen(params) >= 32) return 0;

 	if (Player[playerid][pMute] > 0) return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You can't talk while muted!", "KLAIDA: Jus esate uþtildytas, todël negalite kalbëti!");

    
    if(PlayerInfo[playerid][pApsauga] == 1) return SQLapsauga(playerid);

	if (sscanf(params, "is", giveplayerid, rsn)) return SendClientMessageLang(playerid, COLOR_INFO, "USAGE: /report <playerid> <reason>", "NAUDOJIMAS: /report <playerid> <prieþastis>");
	
 	if(!IsPlayerConnected(giveplayerid))
 		return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Þaidëjas neprisijungæs!");
	if(giveplayerid == playerid)
	    return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You are not able to report yourself!", "KLAIDA: Savæs negalite!");
	    
    if(IsPlayerNPC(giveplayerid)) return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Þaidëjas neprisijungæs!");

    if (giveplayerid == INVALID_PLAYER_ID) return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Neteisingas þaidëjo ID!");

	if(giveplayerid == LastReportedID[playerid]) return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You can't report this player again!", "KLAIDA: Negalite praneðti apie þaidëjà kelis kartus ið eilës!");

	if(IsNumeric(rsn)) return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Reason can't be a number!", "KLAIDA: Prieþastis negali bûti skaièius!");

	GetPlayerName(playerid, PlayerName, 24);
	GetPlayerName(giveplayerid, giveplayer, 24);

    format(str, 128, "/REPORTS: Þaidëjas %s(%d) praneðë apie {1AFF00}%s(%d){FF0000} dël: %s", PlayerName, playerid, giveplayer, giveplayerid, rsn);
	LastReportedID[playerid] = giveplayerid;
	SetTimerEx("UnsetReportID", 60000, 0, "i", playerid);
	foreach(Player, i)
	{
		if (Player[i][pAdmin] > 0)
		{
		    SendClientMessage(i, COLOR_ERROR, str);
		    PlayerPlaySound(i, 1132, 0.0, 0.0, 0.0);
			switch (PlayerInfo[i][pKalba])
			{
            	case 0: GameTextForPlayer(i, "~n~~n~~n~~n~~n~~n~~n~~n~~n~~b~Report ~y~Received~b~.", 3000, 3);
            	case 1: GameTextForPlayer(i, "~n~~n~~n~~n~~n~~n~~n~~n~~n~~b~Gautas ~y~skundas~b~", 3000, 3);
			}
		}
	}
	
    switch (PlayerInfo[playerid][pKalba])
    {
    	case 0:
     	{
      		format(str, 128, "Your report about %s(%i) has been sent to all administrators online.", giveplayer, giveplayerid);
        	SendClientMessage(playerid, COLOR_INFO, str);
     	}
      	case 1:
       	{
        	format(str, 128, "Jûsø praneðimas apie %s(%i) buvo iðsiøstas visiems prisijungusiems administratoriams.", giveplayer, giveplayerid);
         	SendClientMessage(playerid, COLOR_INFO, str);
       	}
    }
    AddNewReport(playerid, PlayerName, giveplayerid, giveplayer, rsn);
	return 1;
}

