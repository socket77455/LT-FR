COMMAND:moneta(playerid, params[])
{
	new
		msg[128],
		name[24],
		id = INVALID_PLAYER_ID;

	// patikrinam ar þaidëjas yra pakviestas mesti monetà
	if(CoinInviter[playerid] != INVALID_PLAYER_ID)
	{
		
		id = CoinInviter[playerid];

		// patikrinam, ar pasiûlymà atsiuntæs þaidëjas nepabëgo ið serverio
		if(IsPlayerConnected(id))
		{
			// patikrinam ar dar nepasibaigë laikas
			if(CoinTime[id] > GetTickCount() - COIN_EXPIRE_TIME * 1000)
			{
				// laikas dar nesibaigë, patikrinam kà paraðë þaidëjas
				if(isnull(params))
				{
					// þaidëjas sutiko loðti, pradedam þaidimà
					new 
						flip, // 0 - skaièius, 1 - herbas, 2 - nieko (tipo briauna Xd).
						baigtis[50],
						cashAmmount = CoinAmmount[id];

					// Ámanomos monetos þaidimo baigtys (tik flip == 2 atveju)
					new 
						galimosBaigtys[][50] =
						{
							"Moneta ákrito á ðuliná",
							"Monetà pasiëmë benamis",
							"Monetà pervaþiavo tankas",
							"Monetà nusineðë varna",
							"Moneta niekad nenusileido..",
							"ZZZZZZZZZZZZZZZZOLTAN"
						};

					// iðrenkame random skaièiø nuo 0 iki 99
					switch(random(100))
					{
						case 0..39:	// 40proc tikimybë, kad iðkris skaièius
						{
							flip = 0;
						}
						case 40..89: // 40proc tikimybë, kad iðkris herbas
						{
							flip = 1;
						}
						default: // 10proc tikimybë, kad nieko neiðkris
						{
							flip = 2;
							strcat(baigtis, galimosBaigtys[random(sizeof(galimosBaigtys))]);
						}
					}
					GetPlayerName(id, name, sizeof(name));
					format(msg, sizeof(msg), "[Moneta] %s metë monetà. %s.", name, (!flip) ? ("Iðkrito skaièius") : ((flip == 1) ? ("Iðkrito herbas") : (baigtis)));
					SendClientMessage(playerid, COLOR_INFO, msg);
					SendClientMessage(id, COLOR_INFO, msg);

					// apdovanojame þaidëjus (arba ne)
					if(flip == 2) // kaip ir lygiosios
					{
						SendClientMessage(playerid, COLOR_INFO, "[Moneta] Deja, bet niekas nelaimëjo.");
						SendClientMessage(id, COLOR_INFO, "[Moneta] Deja, bet niekas nelaimëjo.");
					}
					else if(flip == CoinChoise[id]) // laimëjo pasiûlæs þaidëjas
					{
						// patikrinam ar pralaimëjæs þaidëjas vis dar turi pakankamà pinigø sumà. Jei ne, bus nuskaityti visi likæ pinigai
						new 
							cash = 0;

						if((cash = GetPlayerPCash(playerid)) < cashAmmount)
						{
							cashAmmount = cash;

							GetPlayerName(playerid, name, sizeof(name));
							format(msg, sizeof(msg), "[Moneta] %s prarado dalá pinigø kol laukë monetos metimo. Þaidþiama ið jo likusiø pinigø.");
							SendClientMessage(playerid, COLOR_INFO, msg);
							SendClientMessage(id, COLOR_INFO, msg);
						}

						// duodam pinigus
						GivePlayerPCash(id, cashAmmount);
						GivePlayerPCash(playerid, -cashAmmount);

						format(msg, sizeof(msg), "[Moneta] Sveikiname, laimëjote %d$.", cashAmmount);
						SendClientMessage(id, COLOR_INFO, msg);
						format(msg, sizeof(msg), "[Moneta] Deja, bet pralaimëjote %d$.", cashAmmount);
						SendClientMessage(playerid, COLOR_INFO, msg);
					}
					else // pasiûlæs þaidëjas pralaimëjo
					{
						// patikrinam ar pralaimëjæs þaidëjas vis dar turi pakankamà pinigø sumà. Jei ne, bus nuskaityti visi likæ pinigai
						new 
							cash = 0;

						if((cash = GetPlayerPCash(id)) < cashAmmount)
						{
							cashAmmount = cash;

							format(msg, sizeof(msg), "[Moneta] %s prarado dalá pinigø kol laukë monetos metimo. Þaidþiama ið jo likusiø pinigø.");
							SendClientMessage(playerid, COLOR_INFO, msg);
							SendClientMessage(id, COLOR_INFO, msg);
						}

						// duodam pinigus
						GivePlayerPCash(playerid, cashAmmount);
						GivePlayerPCash(id, -cashAmmount);

						format(msg, sizeof(msg), "[Moneta] Sveikiname, laimëjote %d$.", cashAmmount);
						SendClientMessage(playerid, COLOR_INFO, msg);
						format(msg, sizeof(msg), "[Moneta] Deja, bet pralaimëjote %d$.", cashAmmount);
						SendClientMessage(id, COLOR_INFO, msg);
					}

					// atstatom kintamøjø reikðmes á numatytàsias
					CoinInviter[playerid] = INVALID_PLAYER_ID;
					CoinInvitee[id] = INVALID_PLAYER_ID;
					CoinTime[id] = 0;
					CoinChoise[id] = 0;
					CoinAmmount[id] = 0;
					return 1;
				}
				else
				{
					return SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Uþtenka paraðyti /moneta." );
				}
			}
			else
			{
				// þaidëjas nespëjo paraðyti, nustatytas laikas praëjo.
				CoinInviter[playerid] = INVALID_PLAYER_ID;
			}
		}
		else
		{
			// þaidëjas atsijungë
			CoinInviter[playerid] = INVALID_PLAYER_ID;
		}
	}
	// patikrinam ar þaidëjas pakvietæs kà nors mesti monetà
	else if(CoinInvitee[playerid] != INVALID_PLAYER_ID)
	{
		// patikrinam, ar þaidëjas, kurá kvietëme vis dar prisijungæs
		if(IsPlayerConnected(CoinInvitee[playerid]))
		{
			// patikrinam ar dar nepasibaigë laikas
			if(CoinTime[playerid] > GetTickCount() - COIN_EXPIRE_TIME * 1000)
			{
				return SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Jûs jau esat nusiuntæs pasiûlymà. Laukiama atsakymo." );
			}
			else 
			{
				// laikas jau praëjo
				CoinInvitee[playerid] = INVALID_PLAYER_ID;
			}
		}
		else
		{
			// þaidëjas atsijungë
			CoinInvitee[playerid] = INVALID_PLAYER_ID;
		}
	}

	new 
		suma,
		moneta[10],
		pasirinkimas;	// 0 jei skaièius, 1, jei herbas

	if(sscanf(params, "dds", id, suma, moneta))
	    return SendClientMessage(playerid, COLOR_INFO, "USAGE: /moneta <þaidëjoid> <pinigø suma> <skaièius (S) arba herbas (H)>" );
	else if(id == INVALID_PLAYER_ID || !IsPlayerConnected(id) || id == playerid)
		return SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Nurodytas neteisingas ID arba þaidëjas tokiu ID nëra prisijungæs." );
	else if(suma < 1000 || suma > 1000000)
		return SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Nurodyta bloga suma. Minimali suma - 1000$, maksimali - 1000000$." );
	else if(suma > GetPlayerPCash(playerid))
		return SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Neturite pakankamai pinigø.");
	// patikriname ar teisingai áraðyta monetos pusë
	else if(!strcmp(moneta, "skaièius", true, 8) || !strcmp(moneta, "skaicius", true, 8) || !strcmp(moneta, "s", true, 1))
	{
		// pasirinkta moneta
		pasirinkimas = 0;
	}
	else if(!strcmp(moneta, "herbas", true, 6) || !strcmp(moneta, "h", true, 1))
	{
		// pasirinktas herbas
		pasirinkimas = 1;
	}
	else
	{
		return SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Nurodyta klaidinga monetos pusë (neáraðyta herbas arba skaièius (H arba S).");
	}

	// gaunam kito þaidëjo pinigø likutá
	if(GetPlayerPCash(id) < suma)
		return SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Þaidëjas neturi tiek pinigø.");

	GetPlayerName(id, name, 24);
	format(msg, sizeof(msg), "[Moneta] Iðsiuntëte uþklausimà þaidëjui %s(%d)", name, id);
	SendClientMessage(playerid, COLOR_INFO, msg);

	GetPlayerName(playerid, name, 24);
	format(msg, sizeof(msg), "[Moneta] Þaidëjas %s(%d) siûlo mesti monetà. %s pasirinko %s.", name, playerid, name, (!pasirinkimas) ? ("skaièiø") : ("herbà"));
	SendClientMessage(id, COLOR_INFO, msg);
	format(msg, sizeof(msg), "[Moneta] Jei iðkris %s, laimësite %d$. Norëdami sutikti, raðykit /moneta.", (!pasirinkimas) ? ("herbas") : ("skaièius"), suma);
	SendClientMessage(id, COLOR_INFO, msg);
	SendClientMessage(id, COLOR_INFO, "[Moneta] Apsisprendimui turite "#COIN_EXPIRE_TIME" sekundþiø");

	// iðsaugom pakviesto ir pakvietusio þaidëjø id á kintamuosius
	CoinInviter[id] = playerid;
	CoinInvitee[playerid] = id;
	// gauname laikà, kuomet buvo paraðyta komanda
	CoinTime[playerid] = GetTickCount();
	// iðsaugome sumà ir monetos pusës pasirinkimà
	CoinAmmount[playerid] = suma;
	CoinChoise[playerid] = pasirinkimas;
	return 1;
}

