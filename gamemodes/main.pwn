forward OnLoadPlayerWeapons(playerid);
public OnLoadPlayerWeapons(playerid)
{
	if(LoadedWeapons[playerid] == 0)
	{
	    new row_count;
	    cache_get_row_count(row_count);
	    
	    if(row_count != 0)
	    {
	    	ResetPlayerWeapons(playerid);
		}
	    
		new
		    weaponid,
		    ammo;

		for(new i, j = row_count; i < j; i++) // loop through all the rows that were found
		{
			cache_get_value_index_int(i, 1, weaponid);
		    cache_get_value_index_int(i, 2, ammo);

			if(!(0 <= weaponid <= 46)) // check if weapon is valid (should be)
			{
				continue;
			}
			GivePlayerWeapon(playerid, weaponid, ammo);
		}
	
		LoadedWeapons[playerid] = 1;
		mysql_format(g_SQL, query, sizeof(query), "DELETE FROM weapons WHERE userid = %d", Player[playerid][SQL_ID]);
		mysql_tquery(g_SQL, query);
		SetPlayerArmedWeapon(playerid, 0);
	}
	return 1;
}

