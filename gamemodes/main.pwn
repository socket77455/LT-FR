public LockRacers()
{
	foreach (Player, i )
	{
		if(RaceParticipant[i] != 0)
		{
			TogglePlayerControllable(i,0);
			
			new
			    vehicleid = GetPlayerVehicleID ( i );
			    
			if(vehicleid)
				PlayerVehicles[i]=vehicleid;
			else
				PlayerVehicles[i]=0;
		}
	}
}

public UnlockRacers()
{
	foreach (Player, i )
	{
		if(RaceParticipant[i]>0)
		{
			TogglePlayerControllable(i,1);
			if(PlayerVehicles[i] != 0)
			{
				PutPlayerInVehicle(i,PlayerVehicles[i],0);
				PlayerVehicles[i]=0;
			}
		}
	}
}

