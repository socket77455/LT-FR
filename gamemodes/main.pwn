public OnPlayerEnterDynamicCP(playerid, checkpointid)
{
	/* Serverio checkpoint'ai */
	if(checkpointid == pCheckpoint[0] || checkpointid == pCheckpoint[1] || checkpointid == pCheckpoint[2] || checkpointid == pCheckpoint[14])
	{
 		if(PlayerInfo[playerid][pFUN] == 1 || PlayerInfo[playerid][pRKova] == 1 || PlayerInfo[playerid][pKova] == 1) return UsePlayer(playerid);
 		if (GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
 		{
			TogglePlayerControllable(playerid,0);
			SetPlayerShopName(playerid,"");
			PagrAmmuNation(playerid);
		}
	}
	if(checkpointid == pCheckpoint[3] || checkpointid == pCheckpoint[4] || checkpointid == pCheckpoint[15])
	{
		if (GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
 		{
	    	TogglePlayerControllable(playerid, false);
	    	SetCameraBehindPlayer(playerid);
		}
	
		if (GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
 		{
			switch (PlayerInfo[playerid][pKalba])
			{
			case 0: ShowPlayerDialog(playerid, DIALOG_BANK, DIALOG_STYLE_LIST, "Bank", "Withdraw\nDeposit\nDeposit All\nBuy EXP", "Select", "Exit");
			case 1: ShowPlayerDialog(playerid, DIALOG_BANK, DIALOG_STYLE_LIST, "Bankas", "Nuimti\nádëti\nádëti viskà\nPirkti EXP", "Rinktis", "Palikti");
			}
		}
	}
	if(checkpointid == pCheckpoint[5] || checkpointid == pCheckpoint[6] || checkpointid == pCheckpoint[7])
	{
		if (GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
 		{
			if(PlayerInfo[playerid][pFUN] == 1) return UsePlayer(playerid);
			switch (PlayerInfo[playerid][pKalba])
	  		{
			   case 0: ShowMenuForPlayer(DokaiEN, playerid);
			   case 1: ShowMenuForPlayer(DokaiLT, playerid);
			}
			TogglePlayerControllable(playerid, false);
		}
	}
	if(checkpointid == pCheckpoint[8])
	{
		if(PlayerInfo[playerid][pFUN] == 1) return UsePlayer(playerid);
		
		if (GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
 		{
			ShowPlayerDialog(playerid, DIALOG_SPAWN_SHOP, DIALOG_STYLE_LIST, "Shop", "Spawn Weapons\nSpawn Bullets", "Rinktis", "Palikti");
		}
	}
	
	if(checkpointid == pCheckpoint[9])
	{
		switch (PlayerInfo[playerid][pKalba])
	{
		case 0: ShowPlayerDialog(playerid,DIALOG_TRUCK_JOB,DIALOG_STYLE_MSGBOX,"{1AFF00}Trucking mission's","{FFFFFF}If you want to start a truck deliver job press 'Start'","Start", "Close");
		case 1: ShowPlayerDialog(playerid,DIALOG_TRUCK_JOB,DIALOG_STYLE_MSGBOX,"{1AFF00}Furistø darbas","{FFFFFF}Norëdami pradëti Furistø darbà spauskite 'Pradëti'","Pradëti", "Uþdaryti");
	}
	}

	if(checkpointid == pCheckpoint[10])
	{
 		new playerState = GetPlayerState(playerid);
 		if (playerState == PLAYER_STATE_ONFOOT)
 		{
			SetPlayerInterior(playerid, 2);
			SetPlayerPos(playerid,2543.2661,-1315.9263,1031.4219);
		}
	}
	
	if(checkpointid == pCheckpoint[11])
	{
		SetPlayerInterior(playerid, 0);
		SetPlayerPos(playerid,2517.7681,-1272.5436,34.8694);
	}
	
	if(checkpointid == pCheckpoint[12])
	{
	    if(GetPlayerVirtualWorld(playerid) == 0)
	    {
	        new playerState = GetPlayerState(playerid);
	        if (playerState == PLAYER_STATE_ONFOOT)
	        {
	            SetPlayerInterior(playerid,1);
	            SetPlayerPos(playerid,286.1490,-40.6444,1001.5156);
	            SetPlayerFacingAngle( playerid, 359.9000 );
	            SetPlayerShopName(playerid,"");
	            PlayerInfo[playerid][pInterior] = 1;
			}
		}
	}

	if(checkpointid == pCheckpoint[13])
	{
	    if (GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
	    {
	        ShowPlayerDialog(playerid,DIALOG_BUYJETPACK,DIALOG_STYLE_TABLIST_HEADERS,"{FF0000}Jetpack","Weapon\tAmount\tPrice\n{FFFFFF}Jetpack\t{FFFF00}1\t{33AA33}5000$","Pirkti", "Atgal");
	    }
	}
	
	if(checkpointid == pCheckpoint[16])
	{
	    if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
	    {
			ShowModelSelectionMenu(playerid, vehiclelist, "Select Vehicle");
		}
	    if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	    {
			cmd_tele(playerid, "");
		}
	}
	
