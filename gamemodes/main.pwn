forward SpawnVehicle_InfrontOfPlayer(playerid, vehiclemodel, color1, color2);
public SpawnVehicle_InfrontOfPlayer(playerid, vehiclemodel, color1, color2)
{
	new Float:x,Float:y,Float:z;
	new Float:facing;
	new Float:distance;

    GetPlayerPos(playerid, x, y, z);
    GetPlayerFacingAngle(playerid, facing);

    new Float:size_x,Float:size_y,Float:size_z;
	GetVehicleModelInfo(vehiclemodel, VEHICLE_MODEL_INFO_SIZE, size_x, size_y, size_z);

	distance = size_x + 0.5;

  	x += (distance * floatsin(-facing, degrees));
    y += (distance * floatcos(-facing, degrees));

	facing += 90.0;
	
	if(facing > 360.0) facing -= 360.0;
		
	if(PersonalVehicle[playerid] != INVALID_VEHICLE_ID)
	{
		DestroyVehicle(PersonalVehicle[playerid]);
		PersonalVehicle[playerid] = INVALID_VEHICLE_ID;
	}
	
	if(PersonalVehicle[playerid] == INVALID_VEHICLE_ID)
	{
		PersonalVehicle[playerid] = CreateVehicle(vehiclemodel, x, y, z + (size_z * 0.25), facing, color1, color2, -1);
		PlayerPlaySound(playerid, 1085, 0.0, 0.0, 0.0);
		
	    if(IsValidNumberPlateVehicle(PersonalVehicle[playerid]))
	    {
			new numplate[32+1];
			format(numplate,32,"{FF0000}LTFR%04dP",PersonalVehicle[playerid]);
			SetVehicleNumberPlate(PersonalVehicle[playerid], numplate);
		}
		
		SetVehicleToRespawn(PersonalVehicle[playerid]);
		
		SetVehicleVirtualWorld(PersonalVehicle[playerid], GetPlayerVirtualWorld(playerid));
		LinkVehicleToInterior(PersonalVehicle[playerid], GetPlayerInterior(playerid));
	}

	return 1;
}

public OnPlayerModelSelection(playerid, response, listid, modelid)
{
	if(listid == skinlist)
	{
	    if(response)
	    {
	    	SetPlayerSkin(playerid, modelid);
	    	Player[playerid][SpecialSkin] = modelid;
	    }
    	return 1;
	}

	if(listid == vehiclelist)
	{
	    if(response)
	    {
	        if(modelid == 411)
	        {
	            if(PlayerInfo[playerid][pVIP] == 0)
	            {
	                SendClientMessage(playerid, COLOR_ERROR, "***VIP ONLY***");
	                return 1;
	            }
	        }
            SpawnVehicle_InfrontOfPlayer(playerid, modelid, random(255), random(255));
	    }
    	return 1;
	}
	
	if(listid == airvehiclelist)
	{
	    if(response)
	    {
            SpawnVehicle_InfrontOfPlayer(playerid, modelid, random(255), random(255));
	    }
    	return 1;
	}
	
	if(listid == boatlist)
	{
	    if(response)
	    {
            SpawnVehicle_InfrontOfPlayer(playerid, modelid, random(255), random(255));
	    }
    	return 1;
	}
	
	return 1;
}

