//------------------------------------------------------------------------------
public RaceRotation()
{
	if(Ruzkrautos == 1)
		return 0; // RRotation has been disabled
	//ismeta is race

    for(new playerid = 0; playerid<MAX_PLAYERS; playerid++)
    {
        Paliktirace(playerid);
        //print("RaceRotation");
    }

	if(!fexist("race/yrace.rr"))
	{
	    printf("ERROR in  YRACE's Race Rotation (race/yrace.rr): race/yrace.rr doesn't exist!");
	    return -1;
	}

	if(RRotation == -1)
	{
		KillTimer(RotationTimer);
		return -1; // RRotation has been disabled
	}
	if(Participants > 0) return 1; // A race is still active.

    new Cache:resultq = mysql_query ( g_SQL, "SELECT `id` FROM `tracks_races`" );
    new row_count;
    
    cache_get_row_count(row_count);
    cache_delete(resultq);

    new raceid = random(row_count) + 1;
    new rracenames[32], fback, rraces= 0;

	mysql_format(g_SQL, query, sizeof(query), "SELECT `name` FROM `tracks_races` WHERE `id` = %d", raceid);
	new Cache:result = mysql_query(g_SQL, query);

	cache_get_value_name(0, "name", rracenames[rraces], 32);
	cache_delete(result);
	
	if(RRotation > rraces) RRotation = 0;
	fback = LoadRace(rracenames[RRotation]);
	if(fback == -1) printf("ERROR in YRACE's Race Rotation (race/yrace.rr): Race \'%s\' doesn't exist!",rracenames[RRotation]);
	else if (fback == -2) printf("ERROR in YRACE's Race Rotation (race/yrace.rr): Race \'%s\' is created with a newer version of YRACE",rracenames[RRotation]);
	else startrace();
	return 1;
}
