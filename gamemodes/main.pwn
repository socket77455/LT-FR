		{
 			switch (PlayerInfo[playerid][pKalba])
			{
	    		case 0:
	    		{
					ShowPlayerDialog(playerid, DIALOG_LOGIN, DIALOG_STYLE_PASSWORD, "{FF0000}Login", "Wrong password!\nPlease enter your password in the field below:", "Login", "");
				}
	    		case 1:
