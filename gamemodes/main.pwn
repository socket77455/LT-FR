COMMAND:disco(playerid, params[])
{
	if (Player[playerid][pJail] >= 1)
		return YouAreInJail(playerid);

	if(PlayerInfo[playerid][pKova] == 1 || PlayerInfo[playerid][pRKova] == 1 || PlayerInfo[playerid][pMinigun] == 1 || PlayerInfo[playerid][pDisco] == 1 ||
	PlayerInfo[playerid][pDerby] == 1 || JoinedHay[playerid] == 1 || JoinedWet[playerid] == 1 || PlayerInfo[playerid][pSKY] == 1 || PlayerInfo[playerid][pGunGame] == 1 ||
	PlayerInfo[playerid][pFUN] == 1 || PlayerInfo[playerid][pInPlaneStunt] == 1 ||
	PlayerInfo[playerid][pSTUNT] >= 1 || PlayerInfo[playerid][pSTUNT2] >= 1 || PlayerInfo[playerid][pINFERNUS1] >= 1 || PlayerInfo[playerid][pINFERNUS2] >= 1 || PlayerInfo[playerid][pINFERNUS3] >= 1 ||
	PlayerInfo[playerid][pSTUNT3] >= 1 || PlayerInfo[playerid][pCBUG] == 1 ||
	RaceParticipant[playerid]>0 || dragParticipant[playerid]>0 ||
	MaratonasParticipant[playerid]>0 || PlayerInfo[playerid][PVPstate] > 1 && PlayerInfo[playerid][PVPstate] < 4 ||
	PlayerInfo[playerid][TDMstate] > 0 || PlayerInfo[playerid][pFuristas] == 1) return UsePlayer(playerid);
    if(PlayerInfo[playerid][pSTUNT4] >= 1) return UsePlayer(playerid);
    if(PlayerInfo[playerid][pSTUNT5] >= 1) return UsePlayer(playerid);
    if (PlayerInfo[playerid][pSTUNT6] >= 1) return UsePlayer(playerid);
    if(IsPlayerInWarGames[playerid] == true) return UsePlayer(playerid);
   	if(ZaidejasSurvival[playerid] == true) return UsePlayer(playerid);
   	if(InCMEvent[playerid] == true) return UsePlayer(playerid);
   	
    if(PlayerInfo[playerid][pTeleporting] != 0) return AlreadyTeleporting(playerid);

	disco1[playerid] = CreatePlayerObject(playerid, 19129, 9.03520, -55.97390, 75.00000,   0.00000, 0.00000, 0.00000);
	disco2[playerid] = CreatePlayerObject(playerid, 19129, 9.03520, -35.99390, 75.00000,   0.00000, 0.00000, 0.00000);
	disco3[playerid] = CreatePlayerObject(playerid, 19129, -24.81980, -55.97390, 75.00000,   0.00000, 0.00000, 0.00000);
	disco4[playerid] = CreatePlayerObject(playerid, 19129, -24.81980, -36.10490, 75.00000,   0.00000, 0.00000, 0.00000);
    
	Join_Disco(playerid);
	
	new msg_lt[128],msg_en[128],pName[MAX_PLAYER_NAME];
    GetPlayerName(playerid, pName, MAX_PLAYER_NAME);
    format(msg_en,128, "[/DISCO] %s(%i) has joined the Disco Dodgebomb Minigame.", pName, playerid);
    format(msg_lt,128, "[/DISCO] %s(%i) prisijungë á {FF0000}'Disco Dodgebomb' {FF9900}Mini þaidimà.", pName, playerid);

	foreach(Player, i)
	{
		switch (PlayerInfo[i][pKalba])
		{
			case 0: SendClientMessage(i,COLOR_INFO, msg_en);
			case 1: SendClientMessage(i,COLOR_INFO, msg_lt);
		}
	}
	return 1;
}

