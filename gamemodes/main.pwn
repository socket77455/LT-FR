stock EndDerby ( )
{
	new
	    String [ 100 ];

	new
	    winner = -1,
	    cps = -1,
	    i;

	foreachex (Player, i )
    {
        if ( PlayerInfo [ i ] [ pDerby ] == 1 )
        {
			if ( cps < PlayerCPS [ i ] )
        	{
        	    cps = PlayerCPS [ i ];
        	    winner = i;
			}
		}
	}


	SendClientMessageToAll ( COLOR_INFO, "-- Buliø kautynës baigtos!!! --" );

	if ( ( winner != -1 ) && ( CDerbyCP >= 30 ) )
	{
	    GetPlayerName ( winner, String, 24 );
		format ( String, 100, "-- Laimëtoju tapo: %s, surinkæs %d cp. --", String, cps );
		SendClientMessageToAll ( COLOR_YELLOW, String );
		format ( String, 100, "-- Jis laimëjo %d$ ir %d patirties taðkø. --", DERBY_MONEY_AWARD, DERBY_EXP_AWARD );
		SendClientMessageToAll ( COLOR_YELLOW, String );
		GivePlayerPCash ( winner, DERBY_MONEY_AWARD );
		Player [ winner ] [ Exp ] += DERBY_EXP_AWARD;
		Player [ winner ] [ pDerbyLaimeta ] ++;
		GameTextForPlayer(winner, "WINNER!~n~~w~$25000", 8000, 0);
		MissionPassedSound(winner);
	}

	for(new v = 1, j = GetVehiclePoolSize(); v <= j; v++)
	{
	    if ( DerbyVehicle [ v ] )
	    {
	        DestroyVehicle ( v );
	        DerbyVehicle [ v ] = false;
		}
	}

	foreachex (Player, i )
	{
	    if ( ( PlayerInfo [ i ] [ pDerby ] == 1 ) )
	    {
			RemoveFromDerby ( i );
		}
	}
	Derby = DERBY_NONE;
	SetTimer ( "DerbyTimer", DERBY_CYCLE, false );
}

public OnVehicleDeath ( vehicleid )
{
	if ( DerbyVehicle [ vehicleid ] )
	{
	    DestroyVehicle ( vehicleid );
	    DerbyVehicle [ vehicleid ] = false;
	}
	
	if(DeliveryVehicleState == 1)
	{
		if(vehicleid == DeliveryVehicle)
		{
            SetTimer("VehicleDeliveryDestroyed",9250,false);
		}
	}
	
	new Float:P[3]; GetVehiclePos(vehicleid, P[0], P[1], P[2]);
	if(P[2] <= 0.4) return true; //Water Exception.. (added later)

	new model = GetVehicleModel(vehicleid);
	switch(model)
	{
	    case PLANES: return true;
	    case HELIS: return true;
	    default:
	    {
	    	new
				ht = random(EFFECT_RANDOM),
				xang = random(EFFECT_RANDOM)-EFFECT_FIX_DEFAULT,
				yang = random(EFFECT_RANDOM)-EFFECT_FIX_DEFAULT,
				zang = random(EFFECT_RANDOM)
			;
			GetVehiclePos(vehicleid, P[0], P[1], P[2]);
			CreateExplosion(P[0], P[1], P[2]+EFFECT_EXPLOSIONOFFSET, EFFECT_EXPLOSIONTYPE, EFFECT_EXPLOSIONRADIUS);
			SetVehicleAngularVelocity(vehicleid, xang*EFFECT_MULTIPLIER, yang*EFFECT_MULTIPLIER, zang*EFFECT_FIX_Z);
			GetVehicleVelocity(vehicleid, P[0], P[1], P[2]);
			SetVehicleVelocity(vehicleid, P[0], P[1], P[2] + (ht*EFFECT_FIX_Z) );
		}
	}
	
	return true;
}


stock CheckForDerbyVehicle ( )
{
	for(new v = 1, j = GetVehiclePoolSize(); v <= j; v++)
