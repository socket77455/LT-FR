public ZoneUpdate()
{
	for(new z=0; z < MAX_ZONES; z++)
	{
	    if(ZoneTakeOverTeam[z] == -1)
	    {
	        for(new t=0; t < MAX_TEAMS; t++)
	        {
	            if(t == ZoneInfo[z][z_team]) continue;
	            if(ZoneCooldownTime[z] == 1) continue;
	            
	            if(GetMembersInZone(z, t) >= 4) // 4
	            {
	                ZoneTakeOverTeam[z] = t;
	                GangZoneFlashForAll(ZoneInfo[z][z_id], GetTeamColor(t));
	                ZoneTakeOverTime[z] = 0;
	                
               		new zone_msg[128];
					format(zone_msg,128, "%s pradejo teritorijos uzemima!", GetTeamName(t));
					
					foreach(Player, playerid)
					{
					    if(GetPlayerVirtualWorld(playerid) == 0)
					    {
							SendServerMessage(playerid, MSG_STYLE_GANGWAR, zone_msg);
						}
						
						if(GetPlayerTeam(playerid) == t)
						{
						    ShowProgressBarForPlayer(playerid, zonebar[playerid]);
						    SetProgressBarColor(zonebar[playerid], GetTeamColor(t));
						    UpdateProgressBar(zonebar[playerid], playerid);
						}
						
					}
					
				}
			}
		}
		else
