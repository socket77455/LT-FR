    if(PlayerInfo[playerid][PVPstate] > 1 && PlayerInfo[playerid][PVPstate] < 4) return UsePlayer(playerid);
    if(JoinedHay[playerid] == 1) return UsePlayer(playerid);
    if(ZaidejasSurvival[playerid] == true) return UsePlayer(playerid);
    if(InCMEvent[playerid] == true) return UsePlayer(playerid);
    
    if(PlayerInfo[playerid][pDisco] == 1) return UsePlayer(playerid);
    
	if(PlayerInfo[playerid][pVIP] == 0)
