RestartEveryThing ()
{
	new xq, yq, zq;
	new HeyNumber;

	HeySpeed_xy = 2000 / (HAY_Z + 1);
	HeySpeed_z = 1500 / (HAY_Z + 1);
	foreach(Player, i)
	{
	    if (IsPlayerConnected (i))
	    {
	    	WhatHeyLevel[i] = 0;
	    }
