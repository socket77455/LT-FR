stock CheckForAimbot(playerid, Float:fX, Float:fY, Float:fZ, attacked = INVALID_PLAYER_ID)
{
	if(attacked != INVALID_PLAYER_ID)
    {
        if(!IsPlayerAFK(playerid))
        {
	   		if(!IsPlayerInRangeOfPoint(attacked, 3.0, fX, fY, fZ))
	    	{
	    	    if(GetPlayerPing(playerid) < 149)
	    	    {
		          	TimesDetected[playerid]++;
		          	new string[128];

		           	if(TimesDetected[playerid] == 5)
		           	{
						SetTimerEx("RemoveAimbotWarnings",90000,0,"i", playerid);
		            }

		           	else if(TimesDetected[playerid] == 10)
		           	{
		            	format(string, sizeof(string), "{FF0000}WARNING: %s(%d) is POSSIBLY using aimbot! x10 | Packetloss: %.2f% FPS: %d Ping: %d", GetName(playerid), playerid, NetStats_PacketLossPercent(playerid), pFPS[playerid], GetPlayerPing(playerid));
 			          	printf("%s(IP: %s) has received %d/15 aimbot warnings. Packetloss: %.2f% FPS: %d Ping: %d", GetName(playerid), AimPlayerIP(playerid), TimesDetected[playerid], NetStats_PacketLossPercent(playerid), pFPS[playerid], GetPlayerPing(playerid));
		                SendMessageToAdmins(string), string = "\0";
		                DetectedForAimbot{ playerid } = true;
		            }

		            else if(TimesDetected[playerid] == 15)
		            {
		            	format(string, sizeof(string), "{FF0000}WARNING: %s(%d) is POSSIBLY using aimbot! x15 | Packetloss: %.2f% FPS: %d Ping: %d", GetName(playerid), playerid, NetStats_PacketLossPercent(playerid), pFPS[playerid], GetPlayerPing(playerid));
 			          	printf("%s(IP: %s) has received %d/15 aimbot warnings. Packetloss: %.2f% FPS: %d Ping: %d", GetName(playerid), AimPlayerIP(playerid), TimesDetected[playerid], NetStats_PacketLossPercent(playerid), pFPS[playerid], GetPlayerPing(playerid));
		                SendMessageToAdmins(string), string = "\0";
		                TimesDetected[playerid] = 0;
					}
				}
   			}
		}
	}
}

