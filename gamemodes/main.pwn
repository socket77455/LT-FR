COMMAND:gangremove(playerid, params[])
{
    if(Player[playerid][pGAdmin] == 0)
    {
   		switch (PlayerInfo[playerid][pKalba])
		{
			case 0:SendClientMessage(playerid, COLOR_ERROR, "ERROR: You're not a gang leader!");
 			case 1:SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Tu nesi gaujos lyderis!");
		}
    	return 1;
    }
    
	new giveplayerid;
	if(Player[playerid][pGAdmin]>0)
	{
		if(sscanf(params, "d", giveplayerid)) return SendClientMessage(playerid, COLOR_INFO, "USAGE: /gangremove <id>");
    	if(!IsPlayerConnected(giveplayerid)) return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Þaidëjas neprisijungæs!");

	    if(Player[playerid][pGAdmin] == Player[giveplayerid][pGAdmin])
		{
	    	switch (PlayerInfo[playerid][pKalba])
			{
	    		case 0:SendClientMessage(playerid, COLOR_INFO, "INFO: You cannot remove gang leader, try it in server website.");
	    		case 1:SendClientMessage(playerid, COLOR_INFO, "INFO: Paðalinti gaujos lyderá galima tik per VVP.");
			}
	    	return 1;
	    }
	    
	    if(Player[giveplayerid][pGAdmin] == 2)
		{
	    	switch (PlayerInfo[playerid][pKalba])
			{
	    		case 0:SendClientMessage(playerid, COLOR_INFO, "INFO: You cannot remove gang owner!");
	    		case 1:SendClientMessage(playerid, COLOR_INFO, "INFO: Paðalinti gaujos ákûrëjo negalima!");
			}
	    	return 1;
	    }

		if(Player[giveplayerid][pGang] == Player[playerid][pGang])
		{
	    	switch (PlayerInfo[playerid][pKalba])
			{
	    		case 0:SendClientMessage(playerid, COLOR_INFO, "INFO: You have removed one of your gang members.");
	    		case 1:SendClientMessage(playerid, COLOR_INFO, "INFO: Sekmingai paðalinai vienà ið savo gaujos nariø.");
			}

			GameTextForPlayer(giveplayerid, "~p~Gang Removed", 10000, 1);
			Player[giveplayerid][pGang] = 0;
			Player[giveplayerid][pGAdmin] = 0;
			Paliktiviska(giveplayerid);
			NustatytiZaidejaBeTeamo(giveplayerid);
		}
		else
		{
	    	switch (PlayerInfo[playerid][pKalba])
			{
	    		case 0:SendClientMessage(playerid, COLOR_INFO, "INFO: You are not a leader of this player gang.");
	    		case 1:SendClientMessage(playerid, COLOR_INFO, "INFO: Jus nesate ðio þaidëjo gaujos lyderis.");
			}
		}
	}
	return true;
}
