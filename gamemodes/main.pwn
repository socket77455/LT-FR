COMMAND:share ( playerid, params [ ] )
{
	if ( Player [ playerid ] [ HouseID ] == -1 )
	    return SendClientMessage ( playerid, COLOR_RED, "Jûs neturite namo!!" );
	else if ( isnull ( params ) )
	    return SendClientMessage ( playerid, COLOR_YELLOW, "Naudojimas: /share [ playerid / off ]" );

	new
	    hid = Player [ playerid ] [ HouseID ],
  		h 	= GetHouse ( hid );

 	if ( !strcmp ( params, "off" ) )
	{
	    if(Player [ playerid ] [ ShareHouse ] > -1)
	    {
	        Player [ playerid ] [ ShareHouse ] = -1;
	        //UpdateHouses ( hid );
	        SendClientMessage ( playerid, COLOR_RED, "Jûs palikote namà." );
	        return 1;
	    }
	    
	    if ( House [ h ] [ ShareID ] == -1 )
			return SendClientMessage ( playerid, COLOR_RED, "Jûs su niekuo nesidalijate savo namu" );

		format ( House [ h ] [ H_ShareName ], 24, "Server" );
		House [ h ] [ ShareID ] = -1;

		foreach (Player, i )
	    {
	        if ( Player [ i ] [ ShareHouse ] == hid )
	        {
	            Player [ i ] [ ShareHouse ] = -1;
	            break;
			}
		}

		mysql_format ( g_SQL, query, sizeof query, "UPDATE `accounts` SET `ShareHouse` = -1 WHERE `ShareHouse` = %d", hid );
		mysql_tquery ( g_SQL, query );

