	//Nustatom ora
    new w = strval(params);
	SetWeather(w);
	CurrentWeather = w;
	//Iðsaugom
	mysql_format(g_SQL, query, sizeof(query), "UPDATE settings SET oras = '%d' WHERE id = '1'", w);
	mysql_tquery(g_SQL, query);
	//Praneðam visiem
	new msgen[80],msglt[80],pName[MAX_PLAYER_NAME];
