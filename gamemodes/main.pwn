	SendClientMessageLang(playerid, COLOR_INFO, "INFO: You Logged in!", "INFO: Jûs sëkmingai prisijungëte!");

	new name[MAX_PLAYER_NAME];
	GetPlayerName(playerid, name, 24);
	printf("%s(ID: %d) Prisijungë prie savo paskyros.", name, Player[playerid][SQL_ID]);

	PlayerPlaySound(playerid, 1097, 0.0, 0.0, 0.0);
	SetPlayerWeather(playerid, CurrentWeather);
	TogglePlayerControllable(playerid,1);
	
	PlayerInfo[playerid][pLogged] = 1;

 	new zSerial[128];
  	gpci(playerid, zSerial, 128);
	mysql_format(g_SQL, query, sizeof(query), "UPDATE `accounts` SET `serial` = '%s' WHERE `user_id` = %d", zSerial, Player[playerid][SQL_ID]);
	mysql_tquery(g_SQL, query);

 	GetPlayerIp(playerid, PlayerIP, sizeof(PlayerIP));
	mysql_format(g_SQL, query, sizeof(query), "UPDATE `accounts` SET `IP` = '%s', `player_status` = 1 WHERE `user_id` = %d", PlayerIP, Player[playerid][SQL_ID]);
	mysql_tquery(g_SQL, query);

	SetTimerEx("CheckAccountBan",1500,0,"i", playerid);
	SetTimerEx("CheckSerialBan",1500,0,"i", playerid);
	SetTimerEx("AtkurtiZaidejoMute",1500,0,"i", playerid);
}

