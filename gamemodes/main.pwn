COMMAND:count(playerid, params[])
{
	//if (Player[playerid][pAdmin] < 1) return OnlyAdmins(playerid);
	if(Player[playerid][pAdmin] < 1 && !Player [ playerid ] [ pEventMod ]) return OnlyAdmins(playerid);
	
	static amount;
	if(sscanf(params,"i",amount)) return SendClientMessage(playerid,COLOR_INFO,"USAGE: /count <amount>");
	if(iscountactivated == 1) return SendClientMessageLang(playerid,COLOR_ERROR,"ERROR: The countdown is already activated!", "KLAIDA: Atbulinis skaièiavimas jau pradëtas!");
	if(amount > 10 || amount <= 0) return SendClientMessageLang(playerid,COLOR_ERROR,"ERROR: The amount can't be greater than 10", "KLAIDA: Laikmatis negali bûti didesnis uþ 10!");
	
	countworld = GetPlayerVirtualWorld(playerid);
	
	CountTimer = SetTimer("Counttime",1000,true);
	countamount = amount;
	iscountactivated = 1;
	return 1;
}

public Counttime()
{
	if(!iscountactivated){KillTimer(CountTimer);}
	else
	{
	    new string[4];
	    format(string,sizeof(string),"%i",countamount);
	    
	    
		//GameTextForAll(string,1000,3);
		foreach(Player, i)
		{
		    if(GetPlayerVirtualWorld(i) == countworld)
			{
			    GameTextForPlayer(i, string,1000,3);
			}
		}
		
		countamount--;
		if(countamount == -1)
		{
			iscountactivated = 0;
			//GameTextForAll("~g~~h~Go!",1000,3);
			foreach(Player, i)
			{
			    if(GetPlayerVirtualWorld(i) == countworld)
			    {
			        GameTextForPlayer(i, "~g~~h~Go!",1000,3);
			    }
			}
		}
		
	}
	return 1;
}
