	}
	if( inProgress != 2 )
	{
	    new spot;
		foreach(_Minigamer, i )
		{
		    JoinedWet[i] = 1;
		    SetPlayerVirtualWorld(i, WET_PASAULIS);
			ResetPlayerWeapons( i );
			SetPlayerInterior( i, 0 );
