public MaratonasRotation()
{
	if(MaratonasUzkrautos == 1)
		return 0; // MaratonasRRotation has been disabled
//ismeta is Maratonas
    foreach (Player, playerid )
    {
        PaliktiMaratonas(playerid);
    }
//
	if(!fexist("Maratonas/yMaratonas.rr"))
	{
	    printf("ERROR in  YMaratonas's Maratonas Rotation (Maratonas/yMaratonas.rr): Maratonas/yMaratonas.rr doesn't exist!");
	    return -1;
	}

	if(MaratonasRRotation == -1)
	{
		KillTimer(MaratonasRotationTimer);
		return -1; // MaratonasRRotation has been disabled
	}
	if(MaratonasParticipants > 0) return 1; // A Maratonas is still active.

	new File:f, templine[32], rotfile[]="Maratonas/yMaratonas.rr", rMaratonass=-1, rMaratonasnames[32][32], idx, fback;
	f = fopen(rotfile, io_read);
	while(fread(f,templine,sizeof(templine),false))
	{
		idx = 0;
		rMaratonass++;
		rMaratonasnames[rMaratonass]=strtok(templine,idx);
	}
	fclose(f);
	MaratonasRRotation++;
	if(MaratonasRRotation > rMaratonass) MaratonasRRotation = 0;
	fback = LoadMaratonas(rMaratonasnames[MaratonasRRotation]);
	if(fback == -1) printf("ERROR in YMaratonas's Maratonas Rotation (Maratonas/yMaratonas.rr): Maratonas \'%s\' doesn't exist!",rMaratonasnames[MaratonasRRotation]);
	else if (fback == -2) printf("ERROR in YMaratonas's Maratonas Rotation (Maratonas/yMaratonas.rr): Maratonas \'%s\' is created with a newer version of YMaratonas",rMaratonasnames[MaratonasRRotation]);
	else startMaratonas();
	return 1;
}
