public dragRotation()
{
	if(dragUzkrautos == 1)
		return 0; // dragRRotation has been disabled

	//ismeta is drag
    foreach (Player, playerid )
    {
        Paliktidrag(playerid);
    }

	if(!fexist("drag/ydrag.rr"))
	{
	    printf("ERROR in  Ydrag's drag Rotation (drag/ydrag.rr): drag/ydrag.rr doesn't exist!");
	    return -1;
	}

	if(dragRRotation == -1)
	{
		KillTimer(DragRotationTimer);
		return -1; // dragRRotation has been disabled
	}
	if(dragParticipants > 0) return 1; // A drag is still active.

	new File:f, templine[32], rotfile[]="drag/ydrag.rr", rdrags=-1, rdragnames[32][32], idx, fback;
	f = fopen(rotfile, io_read);
	while(fread(f,templine,sizeof(templine),false))
	{
		idx = 0;
		rdrags++;
		rdragnames[rdrags]=strtok(templine,idx);
	}
	fclose(f);
	dragRRotation++;
	if(dragRRotation > rdrags) dragRRotation = 0;
	fback = Loaddrag(rdragnames[dragRRotation]);
	if(fback == -1) printf("ERROR in Ydrag's drag Rotation (drag/ydrag.rr): drag \'%s\' doesn't exist!",rdragnames[dragRRotation]);
	else if (fback == -2) printf("ERROR in Ydrag's drag Rotation (drag/ydrag.rr): drag \'%s\' is created with a newer version of Ydrag",rdragnames[dragRRotation]);
	else startdrag();
	return 1;
}
