stock GetWeaponSlot(iWeapon) {
	switch(iWeapon) {
		case 0, 1:
			return 0;
		case 2, 3, 4, 5, 6, 7, 8, 9:
			return 1;
		case 22, 23, 24:
			return 2;
		case 25, 26, 27:
			return 3;
		case 28, 29, 32:
			return 4;
		case 30, 31:
			return 5;
		case 33, 34:
			return 6;
		case 35, 36, 37, 38:
			return 7;
		case 16, 17, 18, 39, 40:
			return 8;
		case 41, 42, 43:
			return 9;
		case 10, 11, 12, 13, 14, 15:
			return 10;
		case 44, 45, 46:
			return 11;
	}
	return -1;
}

