forward CheckSpeedHacks(playerid);
public CheckSpeedHacks(playerid)
{
	if(!IsPlayerAFK(playerid))
	{
		if(GetPlayerVirtualWorld(playerid) != FUN_ZONOS_PASAULIS)
		{
		    if(GetPlayerInterior(playerid) == 0)
		    {
		    if(PlayerInfo[playerid][pINFERNUS1] == 0 && PlayerInfo[playerid][pINFERNUS2] == 0 && PlayerInfo[playerid][pINFERNUS3] == 0)
		    {
		        if (GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
		        {
		            new model = GetVehicleModel ( GetPlayerVehicleID ( playerid ) );
					if(model == 449 || model == 537 || model == 538)
					{
						return 1;
					}
			
        			new speed = GetVehicleSpeed(GetPlayerVehicleID(playerid));
        			
					if(Player[playerid][pAdmin] >= 1)
					{
						PlayerInfo[playerid][pSpeedWarn] = 0;
					}
					else if(speed > 271)
					{
					    PlayerInfo[playerid][pSpeedWarn]++;
                        
		                if(PlayerInfo[playerid][pSpeedWarn] == 4)
						{
							new string[128];
							format(string, sizeof(string), "{FF0000}WARNING: %s(%d) is POSSIBLY using speed hack! [SPEED: %d FPS: %d] Location: %s", PlayerNick(playerid), playerid, speed,  pFPS[playerid], GetPlayerArea(playerid));
							SendMessageToAdmins(string);
							SetTimerEx("RemoveSpeedWarn",60000,0,"i", playerid);
						}
					}
				}
				}
			}
		}
	}
	return 1;
}

