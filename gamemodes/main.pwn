COMMAND:givecash( playerid, params[ ] )
{
	if (Player[playerid][pJail] >= 1)
		return YouAreInJail(playerid);
		
	if(PlayerInfo[playerid][pApsauga] == 1) return SQLapsauga(playerid);
		
	new
	    id,
	    amount;
	    
    if(PlayerInfo[playerid][PVPstate] != 0)
    {
    	SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You can't give money while in PVP!", "KLAIDA: Negalite duoti pinigu kol dalyvaujate PVP!");
		return 0;
    }

	if ( sscanf( params, "dd", id, amount ) )
	{
    	SendClientMessage(playerid, COLOR_INFO, "USAGE: /givecash <playerid> <money>");
		return true;
	}
	
	if ( Player [ id ] [ Level ] < 1 )
	{
		SendClientMessage ( playerid, 0xFF0000FF, "KLAIDA: ðiam þaidëjui reikia bent 1 lygio!" );
  		return 1;
	}
	
    if( PlayerInfo[ id ][ PVPstate ] != 0 )
    {
    	SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You can't give money while player is in PVP!", "KLAIDA: Negalite duoti pinigu kol þaidëjas dalyvauja PVP!");
		return 1;
    }
    
 	if (!IsPlayerConnected(id)) return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Þaidëjas neprisijungæs!");
	if ( id == INVALID_PLAYER_ID ) return SendClientMessageLang( playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Netinkamas þaidëjo ID!");
	if ( amount <= 1 || amount > 10000000 ) return SendClientMessageLang( playerid, COLOR_ERROR, "ERROR: Invalid money amount. (1 - 10000000)", "KLAIDA: Blogai nurodyta suma. (1 - 10000000)");
	if ( id == playerid ) return SendClientMessageLang( playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Netinkamas þaidëjo ID!");
	if ( GetPlayerPCash( playerid ) < amount ) return SendClientMessageLang( playerid, COLOR_ERROR, "ERROR: You don't have that money!", "KLAIDA: Jus neturite pinigø !");
	else
	{
		new
		    sendername[ MAX_PLAYER_NAME ],
		    giveplayer[ MAX_PLAYER_NAME ],
		    string    [ 65 ];

		GetPlayerName( playerid, sendername, MAX_PLAYER_NAME );
		GetPlayerName(       id, giveplayer, MAX_PLAYER_NAME );

		printf ( "[CASH] %s sumokejo %d lt zaidejui %s", sendername, amount, giveplayer );

		GivePlayerPCash( playerid, -amount );
		GivePlayerPCash( id,        amount );

		switch (PlayerInfo[playerid][pKalba])
        {
            case 0:
            {
                format           ( string, 64, "You have sent %s(%i) $%d", giveplayer, id, amount);
				SendClientMessage(playerid, COLOR_INFO,  string);
            }
            case 1:
            {
                format           ( string, 64, "Jus iðsiuntëte %s(%i) $%d", giveplayer, id, amount);
				SendClientMessage(playerid, COLOR_INFO,  string);
            }
		}

		switch (PlayerInfo[id][pKalba])
        {
            case 0:
            {
                	format           ( string, 64, "You have received $%d from %s(%i)", amount, sendername, playerid);
					SendClientMessage(id, COLOR_INFO,  string);
            }
            case 1:
            {
                	format           ( string, 64, "Jums atsiuntë $%d  -  %s(%i)", amount, sendername, playerid);
					SendClientMessage(id, COLOR_INFO,  string);
            }
        }
        
        SetTimerEx("FloodApsauga",10000,0,"i", playerid);
        PlayerInfo[playerid][pApsauga] = 1;

	}
	return true;
}
