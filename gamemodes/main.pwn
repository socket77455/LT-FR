COMMAND:ganginvite(playerid, params[])
{
    if(strlen(params) >= 32)
		return 0;

    if(Player[playerid][pGAdmin] == 0)
    {
   		switch (PlayerInfo[playerid][pKalba])
		{
			case 0:SendClientMessage(playerid, COLOR_ERROR, "ERROR: You're not a gang leader!");
 			case 1:SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Tu nesi gaujos lyderis!");
		}
    	return 1;
    }

	if(Player[playerid][pGAdmin]>0)
	{
		if ( isnull ( params ) )
		{
    		switch (PlayerInfo[playerid][pKalba])
			{
    			case 0:SendClientMessage(playerid, COLOR_INFO, "USAGE: /ganginvite <id>");
    			case 1:SendClientMessage(playerid, COLOR_INFO, "USAGE: /ganginvite <id>");
			}
			return true;
		}

		new giveplayerid = strval ( params );

		if(!IsPlayerConnected(giveplayerid))
			return SendClientMessage(playerid, COLOR_ERROR, "ERROR: Invalid player ID!");

    	switch (PlayerInfo[playerid][pKalba])
		{
    		case 0:SendClientMessage(playerid, COLOR_INFO, "INFO: Invite has been sent.");
			case 1:SendClientMessage(playerid, COLOR_INFO, "INFO: Pakvietimas prisijungti prie gango iðsiûstas.");
		}

    	switch (PlayerInfo[giveplayerid][pKalba])
    	{
    		case 0:
			{
				SendClientMessage(giveplayerid, COLOR_INFO, "[Gang Info] You just have been invited to join gang.");
    			SendClientMessage(giveplayerid, COLOR_ERROR, "[Gang Info] If you want to join this gang, type: /gang join");
    		}
    		case 1:
			{
				SendClientMessage(giveplayerid, COLOR_INFO, "[Gang Info] Jûs buvote pakviestas prisijungti prie gango.");
    			SendClientMessage(giveplayerid, COLOR_ERROR, "[Gang Info] Norëdamas prisijungti naudok komanda /gang join");
