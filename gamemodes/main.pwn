COMMAND:hitman(playerid,params[])
{
    if(strlen(params) >= 22) return 0;
    if(Player[playerid][pMute] > 0)
	{
    	SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You can not talk while muted!", "KLAIDA: Jus esate uþtildytas, todël negalite kalbëti!");
		return 0;
    }
	new givehitmanid=INVALID_PLAYER_ID,moneys,string[128];

	if(sscanf(params,"dd",givehitmanid,moneys)) return SendClientMessage(playerid,COLOR_INFO,"USAGE: /hitman <playerid> <money>");
	
	if(IsPlayerNPC(givehitmanid)) return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Þaidëjas neprisijungæs!");

	if(moneys > 10000000)
	{
		switch(PlayerInfo[playerid][pKalba])
		{
			case 0:SendClientMessage(playerid, COLOR_ERROR, "ERROR: Your entered ammount is too big!");
			case 1:SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Jûsø ávesta suma yra per didelë!");
		}
		return 1;
	}

	if(moneys > GetPlayerPCash(playerid))
	{
		switch (PlayerInfo[playerid][pKalba])
		{
			case 0:
			{
				SendClientMessage(playerid, COLOR_ERROR, "ERROR: You do not have enough money!");
			}
			case 1:
			{
				SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Jus neturite pakankamai pinigø!");
			}
		}
		return 1;
	}

	if(moneys < 1)
	{
		switch (PlayerInfo[playerid][pKalba])
		{
			case 0:
 			{
				SendClientMessage(playerid, COLOR_ERROR, "ERROR: You need to enter an amount!");
			}
			case 1:
			{
				SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Jums reikia ávesti sumà!");
			}
		}
		return 1;
	}
	if(!IsPlayerConnected(givehitmanid) || givehitmanid==INVALID_PLAYER_ID || givehitmanid==playerid)
	{
		switch (PlayerInfo[playerid][pKalba])
		{
			case 0:
			{
				SendClientMessage(playerid, COLOR_ERROR, "ERROR: Invalid player ID");
			}
			case 1:
			{
				SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Negalima uþdëti pinigø uþ neprisijungusio þaidëjo galvà.");
			}
		}
		return 1;
	}

	Player[givehitmanid][bounty] += moneys;
	GivePlayerPCash(playerid, -moneys);

	new givehitmanplayer[26],sendername[26];
	GetPlayerName(givehitmanid, givehitmanplayer, 24);
	GetPlayerName(playerid, sendername, 24);

	switch (PlayerInfo[playerid][pKalba])
	{
		case 0:
		{
			format(string, 128, "You gave $%d, for %s kill (ID: %d).", moneys, givehitmanplayer,givehitmanid);
			SendClientMessage(playerid, COLOR_INFO, string);
		}
		case 1:
		{
			format(string, 128, "Jûs uþdëjote $%d, uþ %s galvà (aukos ID: %d).", moneys, givehitmanplayer,givehitmanid);
			SendClientMessage(playerid, COLOR_INFO, string);
		}
	}

	new msg_lt[128],msg_en[128];
    format(msg_en,128, "%s is hunting for $%d, hunter %s (total: $%d).", givehitmanplayer, moneys, sendername, Player[givehitmanid][bounty]);
    format(msg_lt,128, "%s yra medþiojamas uþ $%d pinigø sumà, nuo %s (ið viso: $%d).", givehitmanplayer, moneys, sendername, Player[givehitmanid][bounty]);

	foreach (Player, i )
	{
    	switch (PlayerInfo[i][pKalba])
		{
			case 0: SendClientMessage(i,COLOR_INFO, msg_en);
			case 1: SendClientMessage(i,COLOR_INFO, msg_lt);
		}
	}

	switch (PlayerInfo[givehitmanid][pKalba])
	{
		case 0:
		{
			format(string, 128, "Uþ tavo givybæ uþdëta $%d pinigø suma, nuo %s (ID: %d) (viso: $%d).", moneys, sendername, playerid, Player[givehitmanid][bounty]);
			SendClientMessage(givehitmanid, COLOR_ERROR, string);
		}
	    case 1:
	    {
	    	format(string, 128, "You are hunting for $%d, your hunter: %s (ID: %d) (total: $%d).", moneys, sendername, playerid, Player[givehitmanid][bounty]);
			SendClientMessage(givehitmanid, COLOR_ERROR, string);
	    }
	}
	return 1;
}

