COMMAND:afk(playerid,params[])
{
    if(Player[playerid][pAdmin] < 1) return OnlyAdmins(playerid);
    if(JoinedHay[playerid] == 1) return OnlyAdmins(playerid);

    if ( isnull ( params ) )return SendClientMessage(playerid, COLOR_INFO, "USAGE: /afk <playerid>");

    new slapid = strval ( params );
    if(slapid == INVALID_PLAYER_ID)
		return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Þaidëjas nerastas!");
 	if(!IsPlayerConnected(slapid))
 		return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Þaidëjas neprisijungæs!");
    if(slapid == playerid)
		return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Savæs negalima!");
    else
    {
        new AdminName[MAX_PLAYER_NAME],SlapName[MAX_PLAYER_NAME],SlapMessage[128];
        GetPlayerName(slapid, SlapName, 24);
        GetPlayerName(playerid, AdminName, 24);
        format(SlapMessage, sizeof(SlapMessage), "%s has been afk'ed by admin %s!", SlapName, AdminName);
        print ( SlapMessage );
        
        if(GetPlayerVirtualWorld(slapid) == 0)
        {
	        SetPlayerVirtualWorld(slapid, AFK_PASAULIS);
	        TogglePlayerControllable(slapid, 0);
	        ShowPlayerDialog(slapid,DIALOG_AFK_DIALOG,DIALOG_STYLE_MSGBOX,"AFK","Your session has been suspended for being AFK.","Return", "");
		}
  	}
    return 1;
}
