	new giveplayer [ 24 ];

	if(sscanf(params, "id", giveplayerid, alevel))
	{
	    SendClientMessage(playerid, COLOR_INFO, "USAGE: /setlevel <id> <level>");
	    return true;
	}

	GetPlayerName(playerid, PlayerName, 24);
	GetPlayerName(giveplayerid, giveplayer, 24);

    if(!IsPlayerConnected(giveplayerid))
        return SendClientMessage(playerid, COLOR_ERROR, "ERROR: Invalid player ID!");

    new str[128];

	if(PlayerInfo[giveplayerid][pLogged] == 0)
	{
    	switch (PlayerInfo[playerid][pKalba])
        {
            case 0:
            {
                format(str, 128, "ERROR: Player %s(%i) isn't registered, hence the level can't be set!", giveplayer, giveplayerid);
				SendClientMessage(playerid, COLOR_ERROR, str);
            }
            case 1:
