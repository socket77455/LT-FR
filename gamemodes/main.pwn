public endMaratonas()
{
	foreach (Player, i )
	{
		PaliktiMaratonas(i);
	}
    MaratonasUzkrautos = 0;

	for(new i=0;i<MCurrentCheckpoint;i++)
	{
	    MaratonasCheckpoints[i][0]=0.0;
	    MaratonasCheckpoints[i][1]=0.0;
	    MaratonasCheckpoints[i][2]=0.0;
	}
	MCurrentCheckpoint=0;
    foreach (Player, i )
    {
        if(MaratonasParticipant[i]>0)
		{
        	DisablePlayerRaceCheckpoint(i);
        }
		if(MaratonasParticipant[i]==3) //Player was still /ready-locked, unlocking.
		{
				TogglePlayerControllable(i,1);
		}
        MaratonasParticipant[i]=0;
    }
	MaratonasActive=0;
	MaratonasStart=0;
	MaratonasParticipants=0;
	MaratonasPot = 0;
	MaratonasPrizeMP = 3;
    foreach (Player, playerid )
    {
	    if(MaratonasParticipant[playerid]<=1)
		{
			PaliktiMaratonas(playerid);
		    switch (PlayerInfo[playerid][pKalba])
    	    {
    	        case 0: SendClientMessage(COLOR_YELLOW, playerid, "The current FreeRun has been finished.");
    	        case 1: SendClientMessage(COLOR_YELLOW, playerid, "Maratono Lenktynës baigësi.");
    	    }
    	}
    }
	return 1;
}

public MaratonasReadyRefresh()
{
	if(MaratonasActive==1) //No MaratonasCountdown if no Maratonas is active (could occur with /leave)
	{
		new Waiting=0, Ready=0;
		foreach (Player, i )
		{
			if(MaratonasParticipant[i] == 1 || MaratonasParticipant[i] == 2) Waiting++;
			else if(MaratonasParticipant[i] == 3) Ready++;
		}
		if(Waiting==0)
		{
			Maratonascd=5;
			MaratonasCountdownTIMER = SetTimer("MaratonasCountdown",1000,1);
		}
		else if(Ready >= Waiting && MaratonasMajorityDelay > 0 && MaratonasMajStart == 0)
		{
			MaratonasMajStart=1;
			MaratonasMaratonasMajStartTimer = SetTimer("msMaratonasCountdown",10000,1);
			msMaratonascd= MaratonasMajorityDelay;
		}
	}
}

public msMaratonasCountdown()
{
	if(MaratonasStart == 1 || MaratonasMajStart == 0)
	{
		MaratonasMajStart=0;
		KillTimer(MaratonasMaratonasMajStartTimer);
	}
	else
	{
		msMaratonascd-=10;
		if(msMaratonascd <= 0)
		{
			foreach (Player, i )
			{
				if(MaratonasParticipant[i] != 3 && MaratonasParticipant[i] != 0)
				{
        			if(MaratonasParticipant[i]>0)
					{
						DisablePlayerRaceCheckpoint(i);
					}
        			MaratonasParticipant[i]=0;
        			MaratonasParticipants--;
				}
   				else if (MaratonasParticipant[i]!=0)
	   				SendClientMessage(i,COLOR_GREEN,"Pre-Maratonas MaratonasCountdown done, the Maratonas beings!");
			}
  			KillTimer(MaratonasMaratonasMajStartTimer);
	    	Maratonascd=5;
	    	MaratonasCountdownTIMER = SetTimer("MaratonasCountdown",1000,1);
		}
		else
		{
			//
		}
	}
}

