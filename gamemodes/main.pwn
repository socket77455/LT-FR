COMMAND:mute(playerid, params[])
{
	if(PlayerInfo[playerid][pLogged] == 0) return NotLogged(playerid);
	if(Player[playerid][pAdmin] < 1 && !PlayerInfo [ playerid ] [ pMod ]) return OnlyAdmins(playerid);
	if(strlen(params) >= 32) return 0;
	
	new rsn[33];
	new giveplayerid;
	new giveplayer [ 24 ];
	
	if(sscanf(params, "is", giveplayerid, rsn))
	{
    	SendClientMessage(playerid, COLOR_INFO, "USAGE: /mute <playerid> <reason>");
		return true;
	}

	if(strlen(rsn) > 32)
		return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid size of reason (don't go above 32 characters)", "KLAIDA: Per ilgas prieþasties tekstas. (Max: 32 simboliai)");

    if(!IsPlayerConnected(giveplayerid))
        return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Þaidëjas neprisijungæs!");

	if(giveplayerid == playerid)
	    return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You are not able to mute yourself!", "KLAIDA: Tildyti savæ negalima!");

	if(Player[giveplayerid][pAdmin] > Player[playerid][pAdmin])
	    return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You are not able to mute a admin with a higher level than you!", "KLAIDA: Jûs negalite uþtildyti aukðtesnio rango administratoriaus!");

    else if ( Player [ giveplayerid ] [ pMute ] > 0 )
	    return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Player is already muted.", "KLAIDA: Þaidëjas jau uþtildytas!");

	GetPlayerName(playerid, PlayerName, sizeof(PlayerName));
	GetPlayerName(giveplayerid, giveplayer, sizeof(giveplayer));

	new msg_lt[128],msg_en[128];
	format(msg_en,128, "[INFO] %s(%i) has been muted. (15minutes) (reason: %s) (admin: %s)", giveplayer, giveplayerid, rsn, PlayerName);
	format(msg_lt,128, "[INFO] %s(%i) buvo uþtildytas. (15minuèiø) (Prieþastis: %s) (Administratorius: %s)", giveplayer, giveplayerid, rsn, PlayerName);

	foreach (Player, i )
	{
		SendClientMessageLang(i, COLOR_ERROR, msg_en, msg_lt);
	}
	
	GameTextForPlayer(giveplayerid, "~p~muted", 10000, 1);
	Player[giveplayerid][pMute] = 900;
	ShowPlayerRules(giveplayerid);
	PlayerInfo[giveplayerid][pPlayerRules] = 0;
	PlayerPlaySound(giveplayerid, 24816, 0.0, 0.0, 0.0);
    SavePlayerMute(giveplayerid);
	return true;
}

