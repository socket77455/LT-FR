public OnPlayerExitVehicle(playerid, vehicleid)
{

    if(PetrolJob[playerid] > 0)
    {
        SendClientMessage(playerid, COLOR_RED, "KLAIDA: Tu palikai savo darbà, todël tau nebus sumokëta!");
        PetrolJob[playerid] = 0;
        PlayerInfo[playerid][pFuristas] = 0;
        DisablePlayerCheckpoint(playerid);
    }

	for(new x=0; x<MAX_PLAYERS; x++)
	{
		if(GetPlayerState(x) == PLAYER_STATE_SPECTATING && AccInfo[x][SpecID] == playerid && AccInfo[x][SpecType] == ADMIN_SPEC_TYPE_VEHICLE)
		{
			TogglePlayerSpectating(x, 1);
 			PlayerSpectatePlayer(x, playerid);
 			AccInfo[x][SpecType] = ADMIN_SPEC_TYPE_PLAYER;
		}
	}
	SyncVeh[vehicleid] = true;
	return 1;
}
//==============================================================================
stock GetVehicleWithinDistance(Float:x1, Float:y1, Float:z1, Float:dist, &veh)
{
	new Float:x2, Float:y2, Float:z2, Float:d;
	
	for(new i = 1, j = GetVehiclePoolSize(); i <= j; i++)
	{
		if(GetVehicleModel(i) > 0)
		{
			GetVehiclePos(i, x2, y2, z2);
			x2 -= x1; y2 -= y1; z2 -= z1;
			d = x2*x2 + y2*y2 + z2*z2;
			if(d < dist)
			{
				veh = i;
				dist = d;
			}
		}
	}
}
//==============================================================================
stock IsVehicleRcTram(vehicleid)
{
	switch(GetVehicleModel(vehicleid))
	{
		case D_TRAM, RC_GOBLIN, RC_BARON, RC_BANDIT, RC_RAIDER, RC_TANK: return 1;
	}
	return 0;
}
