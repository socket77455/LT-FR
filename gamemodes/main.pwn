COMMAND:pvp(playerid, params[])
{
	if(Player[playerid][pJail] >= 1)
		return YouAreInJail(playerid);

	if(PlayerInfo[playerid][pApsauga] == 1) return SQLapsauga(playerid);
	if(IsPlayerInWarGames[playerid] == true) return UsePlayer(playerid);
	if(ZaidejasSurvival[playerid] == true) return UsePlayer(playerid);
	if(InCMEvent[playerid] == true) return UsePlayer(playerid);
	

	if(PlayerInfo[playerid][pKova] == 1 || PlayerInfo[playerid][pRKova] == 1 || PlayerInfo[playerid][pMinigun] == 1 || PlayerInfo[playerid][pDisco] == 1 ||
		PlayerInfo[playerid][pDerby] == 1 || JoinedHay[playerid] == 1 || JoinedWet[playerid] == 1 || PlayerInfo[playerid][pSKY] == 1 || PlayerInfo[playerid][pGunGame] == 1 ||
		PlayerInfo[playerid][pFUN] == 1 || PlayerInfo[playerid][pInPlaneStunt] == 1 ||
		PlayerInfo[playerid][pSTUNT] >= 1 || PlayerInfo[playerid][pSTUNT2] >= 1 || PlayerInfo[playerid][pINFERNUS1] >= 1 || PlayerInfo[playerid][pINFERNUS2] >= 1 || PlayerInfo[playerid][pINFERNUS3] >= 1 ||
		PlayerInfo[playerid][pSTUNT3] >= 1 || PlayerInfo[playerid][pCBUG] == 1 ||
		RaceParticipant[playerid]>0 || dragParticipant[playerid]>0 ||
		MaratonasParticipant[playerid]>0 || PlayerInfo[playerid][PVPstate] > 1 && PlayerInfo[playerid][PVPstate] < 4 ||
		PlayerInfo[playerid][TDMstate] > 0 || PlayerInfo[playerid][pFuristas] == 1 || PlayerInfo[playerid][pInBMXgame] == 1)
		UsePlayer(playerid);
	else
	{
		new pid, Suma, bo, msg[128];
		if(sscanf(params, "iii", pid, Suma, bo))
		{
			SendClientMessage(playerid, COLOR_INFO, "USAGE: /pvp <playerid> <money> <best of>");
			SendClientMessage(playerid, COLOR_INFO, "INFO: 1 - best of one ; 2 - best of three ; 3 - best of five");
			return 1;
		}
		if( bo > 3 || bo == 0 ) return SendClientMessage(playerid, COLOR_ERROR, "ERROR: Best Of number cannot be greater than 3 or lower than 1!");
		if(pid == INVALID_PLAYER_ID)
		{
			switch(PlayerInfo[playerid][pKalba])
			{
				case 0:SendClientMessage(playerid, COLOR_ERROR, "ERROR: Player not found.");
				case 1:SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Þaidëjas nerastas.");
			}
			return 1;
		}
		if(pid == playerid)
		{
			switch(PlayerInfo[playerid][pKalba])
			{
				case 0:SendClientMessage(playerid, COLOR_ERROR, "ERROR: You cant invite yourself.");
				case 1:SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Jûs negalite savæs kviesti dvikovos.");
			}
			return 1;
		}
		if(PlayerInfo[pid][pKova] == 1 || PlayerInfo[pid][pRKova] == 1 || PlayerInfo[pid][pMinigun] == 1 || PlayerInfo[pid][pDisco] == 1 || IsPlayerInWarGames[pid] == true || ZaidejasSurvival[pid] == true || InCMEvent[pid] == true ||
		PlayerInfo[pid][pDerby] == 1 || JoinedHay[pid] == 1 || JoinedWet[pid] == 1 || PlayerInfo[pid][pSKY] == 1 ||
		PlayerInfo[pid][pFUN] == 1 || PlayerInfo[pid][pInPlaneStunt] == 1 ||
		PlayerInfo[pid][pSTUNT] >= 1 || PlayerInfo[pid][pSTUNT2] >= 1 || PlayerInfo[pid][pINFERNUS1] >= 1 || PlayerInfo[pid][pINFERNUS2] >= 1 || PlayerInfo[playerid][pINFERNUS3] >= 1 ||
		PlayerInfo[pid][pSTUNT3] >= 1 || PlayerInfo[pid][pCBUG] == 1 || PlayerInfo[pid][pInBMXgame] == 1 ||
		Player[pid][pJail] >= 1 ||
		RaceParticipant[pid]>0 || dragParticipant[pid]>0 ||
		MaratonasParticipant[pid]>0 || PlayerInfo[pid][TDMstate] > 0 || PlayerInfo[pid][pFuristas] == 1)
		{
			switch(PlayerInfo[playerid][pKalba])
			{
				case 0:SendClientMessage(playerid, COLOR_ERROR, "ERROR: Player is at mini-game already!");
				case 1:SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Þaidëjas jau kaþkur dalyvauja!");
			}
			return 1;
		}
		if(Suma < 1000)
		{
			switch(PlayerInfo[playerid][pKalba])
			{
				case 0:SendClientMessage(playerid, COLOR_ERROR, "ERROR: You can't bet less than 1000$");
				case 1:SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Jûs negalite statyti maþiau nei 1000$");
			}
			return 1;
		}
		if(Suma > GetPlayerPCash(playerid))
		{
			switch(PlayerInfo[playerid][pKalba])
			{
				case 0:SendClientMessage(playerid, COLOR_ERROR, "ERROR: You have not enough money.");
				case 1:SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Jums nepakanka pinigø.");
			}
			return 1;
		}
		if(Suma > GetPlayerPCash(pid))
		{
			switch(PlayerInfo[pid][pKalba])
			{
				case 0:SendClientMessage(playerid, COLOR_ERROR, "ERROR: Player dosen't have enough money.");
				case 1:SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: Þaidëjui nepakanka pinigø.");
			}
			return 1;
		}
		if(PlayerInfo[pid][PVPstate] == 1)
		{
			switch(PlayerInfo[playerid][pKalba])
			{
				case 0:SendClientMessage(playerid, COLOR_ERROR, "ERROR: This player is already invited to PVP");
				case 1:SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: ðis þaidëjas jau yra pakviestas á dvikovà");
			}
			return 1;
		}
		if(Player[pid][PVPoff] == 1)
		{
			switch(PlayerInfo[playerid][pKalba])
			{
				case 0:SendClientMessage(playerid, COLOR_ERROR, "ERROR: This player has blocked PVP invites.");
				case 1:SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: ðis þaidëjas uþblokavo PVP pakvietimus.");
			}
			return 1;
		}
		if(PlayerInfo[pid][PVPstate] == 2 || PlayerInfo[pid][PVPstate] == 3)
		{
			switch(PlayerInfo[playerid][pKalba])
			{
				case 0:SendClientMessage(playerid, COLOR_ERROR, "ERROR: This player is in PVP. Try to invite later.");
				case 1:SendClientMessage(playerid, COLOR_ERROR, "KLAIDA: ðis þaidejas dalyvauja dvikovoje. Mëginkite pakviesti vëliau.");
			}
			return 1;
		}

		new bestof;
		if( bo == 1 ) bestof = 1;
		if( bo == 2 ) bestof = 3;
		if( bo == 3 ) bestof = 5;

		PlayerInfo[playerid][PVPcost] = Suma;
		PlayerInfo[playerid][PVP] = pid;
		PlayerInfo[playerid][PVPstate] = 1;
		PlayerInfo[playerid][PVPBO] = bestof;
		PlayerInfo[pid][PVPcost] = Suma;
		PlayerInfo[pid][PVP] = playerid;
		PlayerInfo[pid][PVPstate] = 1;
		PlayerInfo[pid][PVPBO] = bestof;

		switch(PlayerInfo[pid][pKalba])
		{
			case 0:
			{
				format(msg, 128, "%s invites you to fight for %d$.\nBest Of %d", PlayerNick(playerid), Suma, bestof);
				ShowPlayerDialog(pid, DIALOG_PVP, DIALOG_STYLE_MSGBOX, "PVP Challenge", msg, "Accept", "Decline");
			}
			case 1:
			{
				format(msg, 128, "%s kvieèia jus á dvikovà dël %d$.\nBest Of %d", PlayerNick(playerid), Suma, bestof);
				ShowPlayerDialog(pid, DIALOG_PVP, DIALOG_STYLE_MSGBOX, "Dvikova", msg, "Sutikti", "Atðaukti");
			}
		}
	}
	PlayerInfo[playerid][pApsauga] = 1;
	SetTimerEx("FloodApsauga",5000,0,"i", playerid);
	return 1;
}

COMMAND:pvpoff(playerid, params[])
