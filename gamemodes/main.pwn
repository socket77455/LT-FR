			case 0:SendClientMessage(playerid, COLOR_RED, "You are already blocked PVP invites.");
			case 1:SendClientMessage(playerid, COLOR_RED, "Jûs jau uþblokavæs PVP pakvietimus.");
		}
	}
	return 1;
}

public AFK_TimeUpdateForAll()
{
	foreach (Player, i)
	{
	    if(IsPlayerAFK(i))
	    {
			Sec[i]++;
			IsAFK{ i } = true;

			if(Sec[i] == 60)
			{
			    Sec[i] = 0;
				Min[i]++;
			}

			if(Min[i] == 15 && Sec[i] == 0)
			{
			    if(GetPlayerVirtualWorld(i) == 0)
			    {
	   				SetPlayerVirtualWorld(i, AFK_PASAULIS);
		        	TogglePlayerControllable(i, 0);
		        	ShowPlayerDialog(i,DIALOG_AFK_DIALOG,DIALOG_STYLE_MSGBOX,"AFK","Your session has been suspended for being AFK.","Return", "");
	        	}
			}

			if(Min[i] == 30 && Sec[i] == 0)
			{
				ResetPlayerPropertyInfo(i);
			}
			
			if(Min[i] == 60)
			{
				Min[i] = 0;
                AfkHour[i]++;
			}

			new str[30];
			format(str, 30, "AFK (%02d:%02d:%02d)",AfkHour[i], Min[i], Sec[i]);
            SetPlayerChatBubble(i, str, 0xA79033FF, 25.0, 1000);

           	if(JoinedWet[i] == 1)
			{
				Iter_Remove(_Minigamer, i );
				Minigamer_{ i } = false;
				Paliktiviska(i);
				SetPlayerHealth(i, 50);
			}
			
			if(IsPlayerInWarGames[i] == true)
			{
       			Hydra_ExitPlayer(i);
       			SetPlayerHealth(i, 50);
			}
			
			if(ZaidejasSurvival[i] == true)
			{
       			EXIT_SURVIVAL(i);
       			Paliktiviska(i);
       			SetPlayerHealth(i, 50);
			}
			
            if(InCMEvent[i] == true)
			{
				ColorMatch_Eliminate(i);
				Paliktiviska(i);
			}

		}
	    if(!IsPlayerAFK(i))
	    {
	        Sec[i] = 0;
	        Min[i] = 0;
			AfkHour[i] = 0;
			IsAFK{ i } = false;
		}
	}
	return 1;
}

