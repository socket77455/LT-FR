public LoadMaratonas(tmp[])
{
	new Maratonas_name[32],templine[512];
	format(CMaratonasName,sizeof(CMaratonasName), "%s",tmp);
	format(Maratonas_name,sizeof(Maratonas_name), "Maratonas/%s.yr",tmp);
	if(!fexist(Maratonas_name)) return -1; // File doesn't exist
	CMaratonasFile=Maratonas_name;
    MCurrentCheckpoint=-1; MaratonasRLenght=0; MaratonasRLenght=0;
	new File:f, i;
	f = fopen(Maratonas_name, io_read);
	fread(f,templine,sizeof(templine));
	if(templine[0] == 'Y') //Checking if the Maratonasfile is v0.2+
	{
		new fileversion;
	    strtok(templine,i); // read off YMaratonas
		fileversion = strval(strtok(templine,i)); // read off the file version
		if(fileversion > MaratonasFILE_VERSION) return -2; // Check if the Maratonas is made with a newer version of the Maratonasfile format
		CMaratonasBuilder=strtok(templine,i); // read off MaratonasBUILDER
		OMaratonasmode = strval(strtok(templine,i)); // read off Maratonasmode
		OMaratonaslaps = strval(strtok(templine,i)); // read off amount of laps
		if(fileversion > 1)
		{
			AirMaratonas = strval(strtok(templine,i));   // read off airMaratonas
			MaratonasCPsize = 7.0;//floatstr(strtok(templine,i));    // read off CP size
		}
		else // v1 file format, set to default
		{
			AirMaratonas = 0;
			MaratonasCPsize = 7.0;
		}
		Maratonasmode=OMaratonasmode; Maratonaslaps=OMaratonaslaps; //Allows changing the modes, but disables highscores if they've been changed.
		fread(f,templine,sizeof(templine)); // read off best Maratonas times
		i=0;
		for(new j=0;j<5;j++)
		{
		    TopMaratonasrs[j]=strtok(templine,i);
		    TopMaratonasrTimes[j]=strval(strtok(templine,i));
		}
		fread(f,templine,sizeof(templine)); // read off best lap times
		i=0;
		for(new j=0;j<5;j++)
		{
		    MaratonasTopLapTimes[j]=strval(strtok(templine,i));
		}
	}
	else //Otherwise add the lines as checkpoints, the file is made with v0.1 (or older) version of the script.
	{
		MCurrentCheckpoint++;
		MaratonasCheckpoints[MCurrentCheckpoint][0] = floatstr(strtok(templine,i));
		MaratonasCheckpoints[MCurrentCheckpoint][1] = floatstr(strtok(templine,i));
		MaratonasCheckpoints[MCurrentCheckpoint][2] = floatstr(strtok(templine,i));
		Maratonasmode=0; OMaratonasmode=0; Maratonaslaps=0; OMaratonaslaps=0;   //Enables converting old files to new versions
		MaratonasCPsize = 8.0; AirMaratonas = 0;  			// v2 additions
		CMaratonasBuilder="UNKNOWN";
		for(new j;j<5;j++)
		{
		    MaratonasTopLapTimes[j]=0; TopMaratonasrs[j]="A"; TopMaratonasrTimes[j]=0;
		}
	}
	while(fread(f,templine,sizeof(templine),false))
	{
		MCurrentCheckpoint++;
		i=0;
		MaratonasCheckpoints[MCurrentCheckpoint][0] = floatstr(strtok(templine,i));
		MaratonasCheckpoints[MCurrentCheckpoint][1] = floatstr(strtok(templine,i));
		MaratonasCheckpoints[MCurrentCheckpoint][2] = floatstr(strtok(templine,i));
		if(MCurrentCheckpoint >= 1)
		{
		    MaratonasRLenght+=Distance(MaratonasCheckpoints[MCurrentCheckpoint][0],MaratonasCheckpoints[MCurrentCheckpoint][1],
								MaratonasCheckpoints[MCurrentCheckpoint][2],MaratonasCheckpoints[MCurrentCheckpoint-1][0],
								MaratonasCheckpoints[MCurrentCheckpoint-1][1],MaratonasCheckpoints[MCurrentCheckpoint-1][2]);
		}
	}
	MaratonasLLenght = MaratonasRLenght + Distance(MaratonasCheckpoints[MCurrentCheckpoint][0],MaratonasCheckpoints[MCurrentCheckpoint][1],
								MaratonasCheckpoints[MCurrentCheckpoint][2],MaratonasCheckpoints[0][0],MaratonasCheckpoints[0][1],
								MaratonasCheckpoints[0][2]);
	fclose(f);
	return 1;
}
