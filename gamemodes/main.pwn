forward All();
public All()
{
	foreach (Player, i )
	{
	    new p = GetPlayerPing(i);
	    
	    if(p >= MAX_PING)
	    {
	        if(PlayerInfo[i][PingWarn]<=1)
	        {
	        	if( Player[ i ][ pAdmin ] == 5 ) return false;
	            new string[100];
	        	PlayerInfo[i][PingWarn]++;
	        	switch(PlayerInfo[i][pKalba])
				{
	        	    case 0:
					{
						format(string, 100, "Your ping is too high. (Warnings: %d/3)",PlayerInfo[i][PingWarn]);
						SendClientMessage(i, COLOR_SYSTEM, string);
					}
	        		case 1:
					{
						format(string, 100, "Jûsø ping yra per aukðtas (Áspëjimai: %d/3)",PlayerInfo[i][PingWarn]);
						SendClientMessage(i, COLOR_SYSTEM, string);
					}
				}
	        	return 1;
	        }
	        

	        
	        if(PlayerInfo[i][PingWarn] >= 2)
	        {
	            GetPlayerName(i, PlayerName, 24);
	            
	            new string[120];
	            
	        	switch(PlayerInfo[i][pKalba])
				{
	        	    case 0:
					{
						SendClientMessage(i, COLOR_SYSTEM, "Your ping is too high. (Warnings: 3/3)");
					}
	        		case 1:
					{
						SendClientMessage(i, COLOR_SYSTEM, "Jûsø ping yra per aukðtas. (Áspëjimai: 3/3)");
					}
				}
				foreach (Player, mp )
	        	{
			    	switch(PlayerInfo[mp][pKalba])
					{
			        	case 0:
						{
							format(string,120,"[INFO] %s has been kicked. (reason: high ping +%d)",PlayerName,MAX_PING);
						}
			        	case 1:
						{
							format(string,120,"[INFO] %s iðmestas. (Prieþastis: didelis ping +%d)",PlayerName,MAX_PING);
						}
					}
					SendClientMessage(mp,COLOR_ERROR,string);
				}
	            //Kick(i);
	            SetTimerEx("KickPlayer",800,0,"i", i);
	        }
	    }
		if(p < MAX_PING)
		{
		    if(PlayerInfo[i][PingWarn] > 0)
			{
			    PlayerInfo[i][PingWarn] --;
			}
		}
	}
	return 1;
}
