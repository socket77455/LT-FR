stock MasinuDokai(playerid)
{
    new
        vehicleid = GetPlayerVehicleID ( playerid ),
        model = GetVehicleModel ( vehicleid );
        
    if ( model == 603 || model == 575 || model == 517 )
	{
		switch (PlayerInfo[playerid][pKalba])
		{
    		case 0:{SendClientMessage(playerid, COLOR_INFO, "[INFO] This vehicle price is too low. You won't get anything for it!");}
    		case 1:{SendClientMessage(playerid, COLOR_INFO, "[INFO] ðios transporto priemonës kaina per maþa, uþ jà nieko negausi.");}
		}
    	TogglePlayerControllable(playerid, true);
    	return 1;
	}

	if ( vehicleid )
    {
    	TogglePlayerControllable(playerid, true);

		new Float:vheal;
		GetVehicleHealth(vehicleid,vheal);
		if(vheal > 2000) return 1;
		new Float:demageCost = (2000-vheal) * (floatround(vehicleInfo[model - 400][veh_cost] / 2000));
		new Float:costed = vehicleInfo[model - 400][veh_cost] - demageCost;

		if(floatround(costed) > 40)
		{
    		new infoCash[128];
    		//new Float:exp = costed/5;

			//if(exp > (Player[playerid][Level]*10)/2)
				//exp = (Player[playerid][Level]*10)/2;

			GivePlayerExp(playerid,300);

  			switch (PlayerInfo[playerid][pKalba])
    		{
    			case 0:
    			{
					format(infoCash, 128, "[INFO] You sold your vehicle for $%d / 150 exp.", floatround(costed));
    				SendClientMessage(playerid, COLOR_INFO, infoCash);
    				format(infoCash, 128, "[INFO] Vehicle state is %d percent", floatround(vheal/10));
    				SendClientMessage(playerid, COLOR_INFO, infoCash);
    			}
			    case 1:
			    {
					format(infoCash, 128, "[INFO] Jûs pardavëte savo transporto priemonæ uþ $%d / 150 exp.", floatround(costed));
				    SendClientMessage(playerid, COLOR_INFO, infoCash);
				    format(infoCash, 128, "[INFO] Transporto priemonës bûklë buvo ávertinta %dproc.", floatround(vheal/20));
				    SendClientMessage(playerid, COLOR_INFO, infoCash);
			    }
		    }

		    format(infoCash, 128, "~y~$%d~n~+150exp", floatround(costed));
			GameTextForPlayer(playerid, infoCash, 3000, 5);
			GivePlayerPCash(playerid, floatround(costed));
			SetVehicleToRespawn(GetPlayerVehicleID(playerid));
			Player[playerid][pCarsDelivered]++;
		}
		else
		{
			switch (PlayerInfo[playerid][pKalba])
		    {
		    	case 0:
		    	{
		    		SendClientMessage(playerid, COLOR_INFO, "[INFO] This vehicle state is too low. You won't get anything for it!");
	    		}
		    	case 1:
		    	{
		    		SendClientMessage(playerid, COLOR_INFO, "[INFO] ðios transporto priemonës bûklë kritinë, uþ jà nieko negausi.");
		    	}
		    }
		}
  	}
    else
    {
    	switch (PlayerInfo[playerid][pKalba])
    	{
    		case 0:
    		{
    			SendClientMessage(playerid,COLOR_INFO, "[INFO] This is the place where you can sell vehicles and get money /exp for it.");
    		}
    		case 1:
    		{
    			SendClientMessage(playerid,COLOR_INFO, "[INFO] èia ta vieta, kur gali parduoti savo transporto priemonæ, ir gauti pinigø / patirties taðkø uþ jà.");
    		}
    	}
    	TogglePlayerControllable(playerid, true);
    }
    return 1;
}
