COMMAND:ban(playerid, params[])
{
	if(PlayerInfo[playerid][pLogged] == 0) return NotLogged(playerid);
	if(Player[playerid][pAdmin] < 2) return OnlyAdmins(playerid);
	if(strlen(params) >= 32) return 0;
	
	new rsn[128];
	new giveplayerid;
	new giveplayer [ 24 ];
	
	if(sscanf(params, "is", giveplayerid, rsn))
	{
    	SendClientMessage(playerid, COLOR_INFO, "USAGE: /ban <playerid> <reason>");
		return true;
	}

	if(strlen(rsn) > 32)
		return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid size of reason (don't go above 32 characters)", "KLAIDA: Per ilgas prieþasties tekstas. (Max: 32 simboliai)");

    if(!IsPlayerConnected(giveplayerid))
        return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Þaidëjas neprisijungæs!");

	if(giveplayerid == playerid)
	    return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You are not able to ban yourself!", "KLAIDA: Uþblokuoti savæ negalite!");

	if(Player[giveplayerid][pAdmin] > Player[playerid][pAdmin])
	    return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You are not able to ban a admin with a higher level than you!", "KLAIDA: Jûs negalite uþblokuoti aukðtesnio rango administratoriaus!");

    if ( PlayerInfo [ giveplayerid ] [ pKicking ] == 1 )
	    return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Player is getting kicked out.", "KLAIDA: Þaidëjas jau metamas lauk!");
	    
    if(IsPlayerNPC(giveplayerid)) return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Þaidëjas neprisijungæs!");
    if(PlayerInfo[playerid][pLogged] == 0) return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Player has not logged into account yet!", "KLAIDA: Þaidëjas dar nëra neprisijungæs prie savo paskyros!");

	GetPlayerName(playerid, PlayerName, 24);
	GetPlayerName(giveplayerid, giveplayer, 24);

	new msg_lt[128],msg_en[128];
	format(msg_en,128, "[INFO] %s(%i) has been banned. (reason: %s) (admin: %s)", giveplayer, giveplayerid, rsn, PlayerName);
	format(msg_lt,128, "[INFO] %s(%i) buvo uþblokuotas. (Prieþastis: %s) (Administratorius: %s)", giveplayer, giveplayerid, rsn, PlayerName);

	foreach (Player, i )
	{
		switch (PlayerInfo[i][pKalba])
		{
			case 0: SendClientMessage(i,COLOR_ERROR, msg_en);
			case 1: SendClientMessage(i,COLOR_ERROR, msg_lt);
		}
	}
	GameTextForPlayer(giveplayerid, "~p~BANNED", 10000, 1);
    PlayerInfo[giveplayerid][pKicking] = 1;
    
	new ipaddress[16];
	GetPlayerIp(giveplayerid,ipaddress,16);
	MySQLAddBanRecord(giveplayerid, Player[giveplayerid][SQL_ID], ipaddress, rsn, PlayerName, Player[playerid][SQL_ID]);
	return true;
}
