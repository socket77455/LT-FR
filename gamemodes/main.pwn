COMMAND:createhouse ( playerid, params [ ] )
{
  	if(!IsPlayerAdmin(playerid))
	  	return OnlyAdmins(playerid);
	else if ( isnull ( params ) )
	    return SendClientMessage ( playerid, 0xFFFFFFFF, "Naudojimas: /createhouse [ kaina ]" );

	new
	    Float: X,
	    Float: Y,
	    Float: Z,
	    Price = strval ( params );

	GetPlayerPos 		( playerid, X, Y, Z );

	new
	    hid;

	for ( new h = 0; h < MAX_HOUSES; h ++ )
	{
		mysql_format 			( g_SQL, query, 100, "SELECT * FROM `houses` WHERE `ID` = %d", h );
		new Cache:result = mysql_query         	( g_SQL, query );

	    if ( !cache_num_rows ( ) )
	    {
	        hid = h;
	        break;
		}
		
		cache_delete(result);
	}

	mysql_format 		( g_SQL, query, 200, "INSERT INTO `houses` ( `ID`, `OX`, `OY`, `OZ`, `Price`, `SOX`, `SOY`, `SOZ` ) VALUES ( %d, '%f', '%f', '%f', %d, '%f', '%f', '%f' )", hid, X, Y, Z, Price, X, Y, Z + 0.3 );
	mysql_tquery 		( g_SQL, query );
	new Cache:result1 = mysql_query		( g_SQL, "SELECT * FROM `houses_config`WHERE `ID` = 0" );

	new r;
	cache_get_value_name_int(0, "VWorld", r);

	new
	    vw = r;

	mysql_format   	( g_SQL, query, 300, "UPDATE `houses` SET `Interior` = %d, `VWorld` = %d, `IX` = '%f', `IY` = '%f', `IZ` = '%f', `Owner` = '%s', `Password` = '%s', `HouseName` = '%s', `ShareName` = '%s' WHERE `ID` = %d", Inter [ 0 ] [ i_Interior ], vw, Inter [ 0 ] [ i_X ], Inter [ 0 ] [ i_Y ], Inter [ 0 ] [ i_Z ], "Server", "none", "Parduodamas namas", "Server", hid );
	mysql_tquery    ( g_SQL, query );
	
	mysql_format  	( g_SQL, query, 300, "UPDATE `houses` SET `IntX` = '%f', `IntY` = '%f', `IntZ` = '%f' WHERE `ID` = %d", Inter [ 0 ] [ int_X ], Inter [ 0 ] [ int_Y ], Inter [ 0 ] [ int_Z ], hid );
	mysql_tquery    ( g_SQL, query );
	mysql_tquery    ( g_SQL, "UPDATE `houses_config` SET `VWorld` = `VWorld` + 1 WHERE `ID` = 0" );
	//CreateHouse     ( hid );
	mysql_format(g_SQL, query, sizeof(query), "SELECT * FROM `houses` WHERE `ID` = '%d'", hid);
	mysql_tquery(g_SQL, query, "CreateHouse", "d", hid);
	
	cache_delete(result1);
	return 1;
}

