 			PlayerInfo[playerid][pSTUNT3] = 0;
 			PlayerInfo[playerid][pSTUNT4] = 0;
 			PlayerInfo[playerid][pSTUNT5] = 0;
 			PlayerInfo[playerid][pSTUNT6] = 0;
 			PlayerInfo[playerid][pINFERNUS1] = 0;
 			PlayerInfo[playerid][pINFERNUS2] = 0;
 			PlayerInfo[playerid][pINFERNUS3] = 0;
 			PlayerInfo[playerid][pRUSTLERStunt] = 0;
 			PlayerInfo[playerid][pStuntPlaneStunt] = 0;
 			PlayerInfo[playerid][pHydraStunt] = 0;
 			PlayerInfo[playerid][pMaverickStunt] = 0;
 			PlayerInfo[playerid][pNewsMavStunt] = 0;
 			PlayerInfo[playerid][pHunterStunt] = 0;
 			PlayerInfo[playerid][pSanchezStunt] = 0;
    		PlayerInfo[playerid][pFUN] = 0;
    		PlayerInfo[playerid][pFunTime] = 0;
    		PlayerInfo[playerid][pPlayer] = 1;
    		SetPlayerSpecialAction(playerid,SPECIAL_ACTION_NONE);
    		SetTimerEx("RespawnDriverVehicle",4900,0,"i", playerid);
    		SetTimerEx("Paliktiviska",5000,0,"i", playerid);
    		ShowTeleportMessage(playerid);
    		return 1;
