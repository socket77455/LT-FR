	}
	
    if( GetPlayerState( playerid ) == PLAYER_STATE_WASTED )
		return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Command is temporary disabled because you're wasted.", "KLAIDA: Jûs esate mires!");
