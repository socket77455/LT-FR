public Loaddrag(tmp[])
{
	new drag_name[32],templine[512];
	format(CdragName,sizeof(CdragName), "%s",tmp);
	format(drag_name,sizeof(drag_name), "drag/%s.yr",tmp);
	if(!fexist(drag_name)) return -1; // File doesn't exist
	CDragFile=drag_name;
    DragLDragCurrentCheckpoint=-1; DragRLenght=0; DragRLenght=0;
	new File:f, i;
	f = fopen(drag_name, io_read);
	fread(f,templine,sizeof(templine));
	if(templine[0] == 'Y') //Checking if the dragfile is v0.2+
	{
		new fileversion;
	    strtok(templine,i); // read off Ydrag
		fileversion = strval(strtok(templine,i)); // read off the file version
		if(fileversion > dragFILE_VERSION) return -2; // Check if the drag is made with a newer version of the dragfile format
		CDragBuilder=strtok(templine,i); // read off dragBUILDER
		Odragmode = strval(strtok(templine,i)); // read off dragmode
		Odraglaps = strval(strtok(templine,i)); // read off amount of laps
		if(fileversion > 1)
		{
			Airdrag = strval(strtok(templine,i));   // read off airdrag
			DragCPsize = 10.0;//floatstr(strtok(templine,i));    // read off CP size
		}
		else // v1 file format, set to default
		{
			Airdrag = 0;
			DragCPsize = 10.0;
		}
		dragmode=Odragmode; draglaps=Odraglaps; //Allows changing the modes, but disables highscores if they've been changed.
		fread(f,templine,sizeof(templine)); // read off best drag times
		i=0;
		for(new j=0;j<5;j++)
		{
		    Topdragrs[j]=strtok(templine,i);
		    TopdragrTimes[j]=strval(strtok(templine,i));
		}
		fread(f,templine,sizeof(templine)); // read off best lap times
		i=0;
		for(new j=0;j<5;j++)
		{
		    DragTopLapTimes[j]=strval(strtok(templine,i));
		}
	}
	else //Otherwise add the lines as checkpoints, the file is made with v0.1 (or older) version of the script.
	{
		DragLDragCurrentCheckpoint++;
		dragCheckpoints[DragLDragCurrentCheckpoint][0] = floatstr(strtok(templine,i));
		dragCheckpoints[DragLDragCurrentCheckpoint][1] = floatstr(strtok(templine,i));
		dragCheckpoints[DragLDragCurrentCheckpoint][2] = floatstr(strtok(templine,i));
		dragmode=0; Odragmode=0; draglaps=0; Odraglaps=0;   //Enables converting old files to new versions
		DragCPsize = 8.0; Airdrag = 0;  			// v2 additions
		CDragBuilder="UNKNOWN";
		for(new j;j<5;j++)
		{
		    DragTopLapTimes[j]=0; Topdragrs[j]="A"; TopdragrTimes[j]=0;
		}
	}
	while(fread(f,templine,sizeof(templine),false))
	{
		DragLDragCurrentCheckpoint++;
		i=0;
		dragCheckpoints[DragLDragCurrentCheckpoint][0] = floatstr(strtok(templine,i));
		dragCheckpoints[DragLDragCurrentCheckpoint][1] = floatstr(strtok(templine,i));
		dragCheckpoints[DragLDragCurrentCheckpoint][2] = floatstr(strtok(templine,i));
		if(DragLDragCurrentCheckpoint >= 1)
		{
		    DragRLenght+=Distance(dragCheckpoints[DragLDragCurrentCheckpoint][0],dragCheckpoints[DragLDragCurrentCheckpoint][1],
								dragCheckpoints[DragLDragCurrentCheckpoint][2],dragCheckpoints[DragLDragCurrentCheckpoint-1][0],
								dragCheckpoints[DragLDragCurrentCheckpoint-1][1],dragCheckpoints[DragLDragCurrentCheckpoint-1][2]);
		}
	}
	DragLLenght = DragRLenght + Distance(dragCheckpoints[DragLDragCurrentCheckpoint][0],dragCheckpoints[DragLDragCurrentCheckpoint][1],
								dragCheckpoints[DragLDragCurrentCheckpoint][2],dragCheckpoints[0][0],dragCheckpoints[0][1],
								dragCheckpoints[0][2]);
	fclose(f);
	return 1;
}
