COMMAND:kill(playerid, params[])
{
    if(Player[playerid][pJail] >= 1) return YouAreInJail(playerid);
    
    if(Player[playerid][pTeleAbuse] >= 1)
		return BannedTeleport(playerid);

   	new State=GetPlayerState(playerid);
	if(State == PLAYER_STATE_PASSENGER)
	{
		new model = GetVehicleModel ( GetPlayerVehicleID ( playerid ) );

		if(model == 537 || model == 569)
			return ForbiddenTeleportVehicle(playerid);
    }
    
    if (GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
    {
        // Transfender
        if(IsPlayerInRangeOfPoint(playerid, 25.0, 617.5360,-1.9900,1000.6582))
        {
            SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Please leave the garage!", "KLAIDA: Palikite garaþà!");
        	return 1;
        }
        // Wheel Arch Angels
        if(IsPlayerInRangeOfPoint(playerid, 25.0, 615.2851,-124.2390,997.6119))
        {
            SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Please leave the garage!", "KLAIDA: Palikite garaþà!");
        	return 1;
        }
        // Loco Low Co
        if(IsPlayerInRangeOfPoint(playerid, 25.0, 616.7833,-74.8150,997.7614))
        {
            SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Please leave the garage!", "KLAIDA: Palikite garaþà!");
        	return 1;
        }
    }
    if(GetPlayerVirtualWorld(playerid) == 0)
    {
        Player[playerid][pKillStreak] = 0;
	}
	
    PlayerInfo[playerid][pIsDaying] = 1;

	if(GetPlayerVirtualWorld(playerid) == 0)
	{
		if( Teleporting[playerid] == true )
		{
		RemoveTeleportMessage(playerid);
		Teleporting[playerid] = false;
		}

	    new Float:x, Float:y, Float:z;
	    GetPlayerPos(playerid, x, y, z);

		new name[MAX_PLAYER_NAME], string[128];
		GetPlayerName(playerid, name, sizeof(name));
		format(string, sizeof(string), "{FF9900}INFO: %s(%i) \n{FF0000}panaudojo /KILL!", name, playerid );
		SendTeleportMessageToAdmins(string);

		TeleportMessage[playerid] = Create3DTextLabel(string, COLOR_INFO, x, y, z, 75.0, 0, 0);
		Teleporting[playerid] = true;

	    teleport_timer[playerid] = SetTimerEx("RemoveTeleportMessage",10000,0,"i", playerid);
	}
	PlayerInfo[playerid][pECBUG] = 0;
	KillTimer(spawn_protect[playerid]);
 	PlayerInfo[playerid][pSpawnProtected] = 0;
 	PlayerInfo[playerid][pTeleporting]=0;
	SetPlayerHealth(playerid, 0.0);
	return true;
}
