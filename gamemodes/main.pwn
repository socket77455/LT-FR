stock SendMessageToAdmins(text[])
{
	foreach(Player, i)
	{
		if (Player[i][pAdmin] >= 1)
		{
			if(PlayerInfo[i][pLogged] == 1)
				SendClientMessage(i,0xA9C4E4AA,text);
		}
	}
	return 1;
}
stock SendMessageToEventMods(text[])
{
	foreach(Player, i)
	{
		if(Player[i][pAdmin] >= 3 || Player[i][pEventMod] >= 1)
  		{
			if(PlayerInfo[i][pLogged] == 1)
			SendClientMessage(i,0x1781A8FF,text);
		}
	}
	return 1;
}
stock SendTeleportMessageToAdmins(text[])
{
	foreach(Player, i)
	{
		if (Player[i][pAdmin] >= 1)
		{
		    if (PlayerInfo[i][pTeleMSG] == 1)
				  {
					if(PlayerInfo[i][pLogged] == 1)
					SendClientMessage(i,0xb0f6a0AA,text);
			}
		}
	}
	return 1;
}
stock SendMessageToTeam(text[], darbas)
{
	new KomanduSpalva;
	
	switch(darbas)
	{
		case Team_Police:
	  	{
	   		KomanduSpalva = 0xB0E2FFFF;
	  	}
	 	case Team_Mafia:
	  	{
	   		KomanduSpalva = COLOR_BROWN;
	  	}
	  	case Team_Soldier:
	  	{
	   		KomanduSpalva = COLOR_NAVY;
	  	}
	  	case Team_Racer:
	  	{
	   		KomanduSpalva = COLOR_ORANGERED;
	  	}
	  	case Team_Trucker:
	  	{
	   		KomanduSpalva = 0x93c572FF;
	  	}
	  	case Team_Farmer:
	  	{
	   		KomanduSpalva = COLOR_KHANKI;
	  	}
	  	case Team_Swat:
	  	{
			KomanduSpalva = COLOR_SWAT;
	  	}
	  	case Team_Pilot:
	  	{
			KomanduSpalva = 0xb6fcd5AA;
	  	}
	}
	
	foreach(Player, i)
	{
		if (Player[i][pDarbas] == darbas)
		{
			if (PlayerInfo[i][pLogged] == 1)
				SendClientMessage(i,KomanduSpalva,text);
		}
	}
	return 1;
}
