public OnPlayerTakeDamage(playerid, issuerid, Float: amount, weaponid)
{
    if(issuerid != INVALID_PLAYER_ID)
    {
        if(!CoolDown{ playerid } && ToggleCoolDown[issuerid]) CoolDown{ issuerid } = true, SetTimerEx("ResetCoolDown", 2500, false, "i", issuerid);
        
        if(GetPlayerTeam(issuerid) != GetPlayerTeam(playerid) || GetPlayerTeam(playerid) == 255)
    	{
			if(PlayerInfo[playerid][pSpawnProtected] == 0)
			{

				if(IsPlayerInRangeOfPoint(playerid, 15.5, 288.0167,-34.7613,1001.5156)) // Ammunation SF
				{
				    new Float:Health;
				    GetPlayerHealth(issuerid, Health);
				    SetPlayerHealth(issuerid, Health-15);
				}

		        PlayerInfo[playerid][pGotShot] = 1;
		        KillTimer(DisableTeleportTimer[playerid]);
		        DisableTeleportTimer[playerid] = SetTimerEx("EnableTeleportAfterDamage", 25000, false, "i", playerid);

				if(weaponid != WEAPON_FLAMETHROWER)
				{
					TextDrawHideForPlayer(playerid, dave[playerid]);
					TextDrawHideForPlayer(playerid, dave[issuerid]);
					TextDrawHideForPlayer(playerid, nueme[playerid]);
					TextDrawHideForPlayer(playerid, dave[playerid]);

					TextDrawHideForPlayer(issuerid, dave[playerid]);
					TextDrawHideForPlayer(issuerid, dave[issuerid]);
					TextDrawHideForPlayer(issuerid, nueme[playerid]);
					TextDrawHideForPlayer(issuerid, dave[playerid]);

					KillTimer(DamageTimer[playerid]);
					KillTimer(DamageTimerIssuerid[issuerid]);

					new vardas[MAX_PLAYER_NAME+1],string[128],weaponname[35],vardas1[MAX_PLAYER_NAME+1],string1[128];
			 		GetPlayerName(playerid, vardas, 24);
					GetPlayerName(issuerid, vardas1, 24);
			  		GetWeaponName(weaponid, weaponname, sizeof (weaponname));
			  		shots[issuerid]++;
			  		damage[issuerid] = amount += damage[issuerid];

					if(Player[playerid][pDamageStats] == 1)
			  		{
				  		format(string, sizeof(string), "%s/%s~n~Damage: -%.0f Combo: x%d",vardas1, weaponname, damage[issuerid], shots[issuerid]);
				        TextDrawSetString(nueme[playerid], string);
				        TextDrawShowForPlayer(playerid, nueme[playerid]);

			       		if(Player[playerid][pDamageSound] == 1)
						{
							PlayerPlaySound(playerid,5201,0.0,0.0,0.0);
						}
			     	}
			        if(Player[issuerid][pDamageStats] == 1)
			        {
						format(string1, sizeof(string1), "%s/%s~n~Damage: +%.0f Combo: x%d", vardas, weaponname, damage[issuerid], shots[issuerid]);
				  		TextDrawSetString(dave[issuerid], string1);
				    	TextDrawShowForPlayer(issuerid, dave[issuerid]);

						if(Player[issuerid][pDamageSound] == 1)
						{
							PlayerPlaySound(issuerid,17802,0.0,0.0,0.0);
						}
					}

					DamageTimer[playerid] = SetTimerEx("DestruirTextoDraw", 2000, false, "i", playerid);
					DamageTimerIssuerid[issuerid] = SetTimerEx("DestruirTextoDrawIssuerid", 2000, false, "i",issuerid);
		        }

		        if(GetPlayerVirtualWorld(playerid) == 0)
		        {
		      		if(PlayerInfo[playerid][pTeleporting] == 1)
					{
					    KillTimer(TeleportPlayerTimer[playerid]);
					    KillTimer(TeleportSFTimer[playerid]);
					    KillTimer(TeleportLVTimer[playerid]);
					    KillTimer(TeleportLSTimer[playerid]);
					    KillTimer(TeleportCar1Timer[playerid]);

					    if( Teleporting[playerid] == true )
					    {
			   				RemoveTeleportMessage(playerid);
							Teleporting[playerid] = false;
						}

						PlayerInfo[playerid][pTeleporting] = 0;
						SendClientMessageLang(playerid, COLOR_INFO, "INFO: Teleport has been canceled!", "INFO: Teleportas atðauktas!");
					}
				}
	        }
		}
    }
    return 0;
}

