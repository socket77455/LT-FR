	else if(AirMaratonas == 1) SetPlayerRaceCheckpoint(playerid,3,MaratonasCheckpoints[target][0],MaratonasCheckpoints[target][1],MaratonasCheckpoints[target][2],MaratonasCheckpoints[next][0],
							MaratonasCheckpoints[next][1],MaratonasCheckpoints[next][2],MaratonasCPsize);
