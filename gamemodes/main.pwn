stock GivePlayerExp(playerid,exp)
{
	if(!IsPlayerAFK(playerid))
	{
		new GiveExp;
		new string[128];
		new nextlevel = EXPToNextLevel(playerid);

		if (PlayerInfo[playerid][pVIP]>0)
		{
			if (exp > 100)
			{
				PlayerInfo[playerid][pProc] = exp / 100 * PlayerInfo[playerid][pVIP] * 20;
			}
		}

		if (exp > 100 && PlayerInfo[playerid][pVIP]>0)
		{
			switch (PlayerInfo[playerid][pKalba])
	        {
	            case 0:
	            {
				    format(string, 128, "[VIP RATES] You've got another %d exp because you are a VIP.", PlayerInfo[playerid][pProc]);
					SendClientMessage(playerid,COLOR_INFO,string);
	            }
	            case 1:
	            {
				    format(string, 128, "[VIP PROCENTAI] Jûs gavote dar %d exp dël to, kad esate VIP.", PlayerInfo[playerid][pProc]);
					SendClientMessage(playerid,COLOR_INFO,string);
	            }
			}
		}


		Player[playerid][Exp] = Player[playerid][Exp] + ( exp * 2 ) + PlayerInfo[playerid][pProc] + GiveExp;
		PlayerInfo[playerid][pProc]=0;
		
		if(Player[playerid][Exp] > nextlevel)
		{
			AdvanceToNextLevel(playerid);
			PlaySound(playerid,1137);

			switch (PlayerInfo[playerid][pKalba])
	        {
	            case 0:
	            {
				    format(string, 128, "You have achieved a new level, Your new level is: %d.", Player[playerid][Level]);
					SendClientMessage(playerid,COLOR_INFO,string);
	            }
	            case 1:
	            {
				    format(string, 128, "Jûs perëjote á kità lygá! Jûsø naujas lygis yra %d.", Player[playerid][Level]);
					SendClientMessage(playerid,COLOR_INFO,string);
	            }
			}
		}
	}
	return 1;
}

stock EXPToNextLevel(playerid)
{
	new level = Player[playerid][Level];
	switch(level)
	{
	    case 0: return 1000;
	    case 1: return 2500;
	    case 2: return 5000;
	    case 3: return 10000;
	    case 4: return 12500;
	    case 5: return 15000;
	    case 6: return 20000;
	    case 7: return 25000;
	    case 8: return 50000;
	    case 9: return 75000;
	    default: return 250000;
	}
	return -1;
}
