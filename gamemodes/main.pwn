public OnDialogResponse(playerid, dialogid, response, listitem, inputtext[])
{
	if(!(inputtext[0] == 0 || (inputtext[0] == 1 && inputtext[1] == 0))) for(new strPos; inputtext[strPos] > 0; strPos++) if(inputtext[strPos] == '%') inputtext[strPos] = ' ';
	
	if(dialogid == DIALOG_EMPTY) return 0;
	
	if(dialogid == DIALOG_REGISTER)
	{
		if (!response) return ShowRegisterDialog(playerid);

		if (strlen(inputtext) <= 5)
		{
 			switch (PlayerInfo[playerid][pKalba])
			{
	    		case 0:
	    		{
					return ShowPlayerDialog(playerid, DIALOG_REGISTER, DIALOG_STYLE_PASSWORD, "Registration", "Your password must be longer than 5 characters!\nPlease enter your password in the field below:", "Register", "");
				}
	    		case 1:
	    		{
					return ShowPlayerDialog(playerid, DIALOG_REGISTER, DIALOG_STYLE_PASSWORD, "Registracija", "Tavo slaptaþodis turi bûti ilgesnis nei 5 simboliai!\nPraðome ávesti jûsø norimà slaptaþodá:", "Registruotis", "");
				}
			}
		}
		
		new newsalt[17];
		for(new i; i < 16; i++)
		{
			newsalt[i] = random(94) + 33;
		}
		newsalt[16] = 0;
		Player[playerid][Salt] = newsalt;
		SHA256_PassHash(inputtext, Player[playerid][Salt], Player[playerid][Password], 65);

		orm_save(Player[playerid][ORM_ID], "OnPlayerRegister", "d", playerid);
	}
	
	if(dialogid == DIALOG_LOGIN)
	{
		if (!response) return ShowLoginDialog(playerid);

		new hashed_pass[65];
		SHA256_PassHash(inputtext, Player[playerid][Salt], hashed_pass, 65);

		if (strcmp(hashed_pass, Player[playerid][Password]) == 0)
		{
		    // Prisijungta, kai kuriuos dalykus vistiek nustatom
