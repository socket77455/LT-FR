	mysql_format(g_SQL, query, sizeof(query), "SELECT * FROM `player_mutes` WHERE `player_ip` = '%s'", ipaddress);
	new Cache:result = mysql_query(g_SQL, query);

    if ( cache_num_rows ( ) > 0 )
    {
		new int_dest;
		cache_get_value_name_int(0, "time_left", int_dest);
