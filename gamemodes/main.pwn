			new name[MAX_PLAYER_NAME];
			GetPlayerName(playerid, name, MAX_PLAYER_NAME);
			RaceParticipant[playerid]=0;
			PlaySound(playerid,1139);
	        
			if(Ranking == 1)
			{
				Player[playerid][pLenktynespirmavieta]++;
			}
			else if(Ranking == 2)
			{
				Player[playerid][pLenktynesantravieta]++;
