COMMAND:mapoff(playerid, params[])
{
	if(PlayerInfo[playerid][pLogged] == 0) return NotLogged(playerid);
	if(Player[playerid][pAdmin] < 3 && !Player [ playerid ] [ pEventMod ]) return OnlyAdmins(playerid);
	
	//new tmp[128];
	
	if(GetPlayerVirtualWorld(playerid) != EVENT_PASAULIS)
	{
		SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You are not not in the event zone!", "KLAIDA: Taves nëra event'e!");
		return 1;
	}

	/*
	    SetPlayerMarkerForPlayer(zaidejo_id_kuris_matys, zaidejo_id_kuriam_keisim_spalva, spalva)

		Principas toks:
		    Mums reikës 2 ciklø, kodël? Nes reikia patikrinti visus þaidëjus tarpusavyje. Ciklas cikle padro taip, kad ciklas esantis ciklo viduje bûtø iðkviestas
		    tiek kartø kiek vykdomas pirmasis. Pvz ðitie ciklai eina per visus galimus þaidëjus. Test serve galimi yra 6, antras ciklas daro tapati. Tai reiðkia,
		    kad kodas esantis dviejø ciklø viduje bus vykdomas 36 kartus.
	*/
	foreach(Player, i) {
	    foreach(Player, z) {
	        if(i != z) { //Patikrinam ar ciklø iðkvieèiami þaidëjø ID nesutampa. Mes gi nenorim tikrinti visko sau?
	            if(GetPlayerVirtualWorld(i) == EVENT_PASAULIS && GetPlayerVirtualWorld(z) == EVENT_PASAULIS) { //Patikrinu ar abu þaidëjai yra EVENT vw.
		            if(GetPlayerTeam(i) != GetPlayerTeam(z) && GetPlayerTeam(i) == 50 && GetPlayerTeam(z) == 51) { //Patikrinu ar þaidëjø teamai nesutampa, ir kad jie abu bûtø 50 ir 51 (kad nebûtø bugø).
		                SetPlayerMarkerForPlayer(i, z, 0x00ffff00); //Tai va, kadangi skiriasi teamai ir þaidëjo i teamas yra 50, o z - 51, tai darom taip, kad þaidëjui i nustatytø þaidëjo z spalvà kaip nematomà.
		            }else if(GetPlayerTeam(i) != GetPlayerTeam(z) && GetPlayerTeam(i) == 51 && GetPlayerTeam(z) == 50) { //toliau visiðkai taspats kaip anksèiau, tik atvirkðèiai. þaidëjas i - 51, z - 50
				    	SetPlayerMarkerForPlayer(i, z, 0xff69b400);
					}else if(GetPlayerTeam(i) == GetPlayerTeam(z)) {    //Jeigu þaidëjo teamai sutampa, tai yra jeigu jie vienoje komandoje
					    if(GetPlayerTeam(i) == 50) {    //Patikrinam ar teamas 50 ar 51
					        SetPlayerMarkerForPlayer(i, z, 0xff69b4FF); //Nustatom matomas spalvas tarpusavyje (matomumà duoda dvi paskutines spalvos kodo raides/skaiciai. 00 - nematoma, FF - visiskai matoma
					    }else if(GetPlayerTeam(i) == 51) {
					        SetPlayerMarkerForPlayer(i, z, 0x00ffffFF); //same here
					    }
					}
				}
	        }
	    }
	}
	
	/*
	    Tai va, praeinam pro visus event zonos zaidejus kurie yra kazkurioj komandoj ir vel gi, kaip visada, patikrinam visus galimus variantus, pagal kuriuos deliojam spalvas su defaultine funkcija.
	*/
	
	foreach(Player, i)
	{
		if(GetPlayerVirtualWorld(i) == EVENT_PASAULIS)
		{
		    if(GetPlayerTeam(i) == NO_TEAM)
			{
			    SetPlayerColor(i,invisibleColors[i]);
			    SetPlayerColor(playerid, playerColors[playerid]);
			    SetPlayerMarkerForPlayer(playerid,i, playerColors[i]);
			}
		}
	}
	new msgen[128],msglt[128];
    format(msgen,128, "[EVENT] %s(%i) has turned off map icons!", GetName(playerid), playerid);
    format(msglt,128, "[EVENT] %s(%i) Iðjungë minimap'o ikonëles event pasaulyje!", GetName(playerid), playerid);
	SendClientMessageToAllLang(COLOR_INFO, msgen, msglt);
    return 1;
}
