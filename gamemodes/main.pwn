public StartDerbyCountDown ( )
{
	static
	    count = 6;

	count --;

	new
	    String [ 100 ];

	if ( count > 0 )
	{
	    format ( String, 20, "~w~] ~g~%d ~w~]", count );

     	foreach (Player, i )
	    {
	        if ( ( PlayerInfo [ i ] [ pDerby ] == 1 ) )
	        {
	        	GameTextForPlayer 	( i, String, 1200, 4 );
				PlayerPlaySound     ( i, 1056, 0, 0, 0 );
				//continue;
			}
		}
	    SetTimer ( "StartDerbyCountDown", 1000, false );
	}
	else
	{
	    DCP = random ( 49 );
        foreach (Player, i )
	    {
	        if ( ( PlayerInfo [ i ] [ pDerby ] == 1 ) )
	        {
	            new
	                vid = GetPlayerVehicleID ( i );
	                
	            if ( !vid )
	            {
					RemoveFromDerby ( i );
					continue;
				}
				
				new
				    nitro = GetVehicleComponentInSlot ( vid, CARMODTYPE_NITRO );
				    
				if ( nitro == 1010 )
				    RemoveVehicleComponent ( vid, 1010 );
				    
	        	GameTextForPlayer 		( i, "~g~GO GO GO", 3000, 4 );
				PlayerPlaySound     	( i, 1057, 0, 0, 0 );
				TogglePlayerControllable( i, true );
				PlayerCPS [ i ] = 0;

