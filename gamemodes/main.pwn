COMMAND:bounties(playerid,params[])
{
    new x,bountied,string[128];
    SendClientMessageLang(playerid, COLOR_WHITE, "Online players bounties:", "Prisijungusiø þaidëjø uþmokestis uþ galvas:");
    
    foreach (Player, i )
	{
		if ( Player[i][bounty] > 0 )
		{
            new givehitmanplayer[24];
			GetPlayerName(i, givehitmanplayer, 24);
			format(string, 128, "%s%s(%d): $%d", string,givehitmanplayer,i,Player[i][bounty]);
			bountied++;
			x++;
			if(x > 3)
			{
			    SendClientMessage(playerid, COLOR_YELLOW, string);
			    x = 0;
				format(string, 128, "");
			}
			else
			{
				format(string, 128, "%s, ", string);
			}
		}
	}

	if(x <= 3 && x > 0)
	{
		string[strlen(string)-2] = '.';
	    SendClientMessage(playerid, COLOR_YELLOW, string);
	}
	if(bountied == 0)
	{
	    return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: There is no bounties", "KLAIDA: Nëra uþmokesèiø uþ galvas.");
	}
	return 1;
}
