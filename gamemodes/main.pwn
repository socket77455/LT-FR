public MySQLAddBanRecord(playerid, sqlplayerid, ipaddr[], rsn[], admin[], sqladminid)
{
	new date[256], time[256];
	new escip[16];
	new hour, minute, sec, year, month, day;
	new playernamesql[32];
	GetPlayerName(playerid, playernamesql, sizeof(playernamesql));
	gettime(hour, minute, sec);
	format(time, 256, "%02d:%02d:%02d", hour, minute, sec);
	getdate(year, month, day);
	format(date, 256, "%04d-%02d-%02d", year, month, day);
	mysql_escape_string(ipaddr, escip, 16, g_SQL);
	mysql_format(g_SQL, query, sizeof(query), "INSERT INTO bans (date,time,ip,PlayerName,userid,rsn,admin,adminid) VALUES ('%s','%s','%s','%s',%d, '%s','%s','%d')",date, time, escip, playernamesql, sqlplayerid, rsn, admin, sqladminid);
	mysql_tquery(g_SQL, query);
    TogglePlayerControllable(playerid,0);
    StopAudioStreamForPlayer(playerid);
    PlayerPlaySound(playerid, 35451, 0.0, 0.0, 0.0);
    Kick(playerid);
	return 1;
}
