COMMAND:leave(playerid)
{
    if (Player[playerid][pJail] >= 1)
		return YouAreInJail(playerid);
		
    if (PlayerInfo[playerid][pTeleporting] == 1)
		return AlreadyTeleporting(playerid);
		
 	if(PlayerInfo[playerid][pSpecPlayer] > 1)
 	    return AlreadyTeleporting(playerid);

    if(Player[playerid][pTeleAbuse] >= 1)
 	{
    	if(GetPlayerVirtualWorld(playerid) == 0)
    	{
			BannedTeleport(playerid);
			return 1;
		}
	}
	
    if (GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
    {
        // Transfender
        if(IsPlayerInRangeOfPoint(playerid, 25.0, 617.5360,-1.9900,1000.6582))
        {
            SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Please leave the garage!", "KLAIDA: Palikite garaþà!");
        	return 1;
        }
        // Wheel Arch Angels
        if(IsPlayerInRangeOfPoint(playerid, 25.0, 615.2851,-124.2390,997.6119))
        {
            SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Please leave the garage!", "KLAIDA: Palikite garaþà!");
        	return 1;
        }
        // Loco Low Co
        if(IsPlayerInRangeOfPoint(playerid, 25.0, 616.7833,-74.8150,997.7614))
        {
            SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Please leave the garage!", "KLAIDA: Palikite garaþà!");
        	return 1;
        }
    }
	
	
   	new State=GetPlayerState(playerid);
	if(State == PLAYER_STATE_PASSENGER)
	{
		new model = GetVehicleModel ( GetPlayerVehicleID ( playerid ) );

		if(model == 537 || model == 569)
			return ForbiddenTeleportVehicle(playerid);
    }
	
	if(PlayerInfo[playerid][pIsDaying] == 1)
 	{
	    SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Too fast!", "KLAIDA: Neraðykite taip greitai!");
	    return 1;
	}

 	if(GetPlayerVirtualWorld(playerid) == 0)
    {
		if(PlayerInfo[playerid][pGotShot] == 1)
		{
		    SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You can't teleport for 25 seconds after taking damage from other player!", "KLAIDA: Teleportuotis negalima 25 sek. jeigu buvote paðautas kito þaidëjo!");
			return 1;
		}
	}


	PlayerInfo[ playerid ][ pDMArena ] = 0;
	
	if(IsPlayerInWarGames[playerid] )
	{
		Hydra_ExitPlayer(playerid);
		//Paliktiviska(playerid);
		return 1;
	}
	
	if(ZaidejasSurvival[playerid])
	{
		EXIT_SURVIVAL(playerid);
		Paliktiviska(playerid);
		return 1;
	}
	
	if(InCMEvent[playerid] == true)
	{
		ColorMatch_Eliminate(playerid);
		Paliktiviska(playerid);
		return 1;
	}
   	
	if (State == PLAYER_STATE_DRIVER)
	{
	    new model = GetVehicleModel ( GetPlayerVehicleID ( playerid ) );
	        
    	if (model == 520 || model == 432 || model == 447 || model == 425)
			return ForbiddenTeleportVehicle(playerid);
    }
    
	if (PlayerInfo[playerid][PVPstate] == 2 || PlayerInfo[playerid][PVPstate] == 3)
	{
		SetPlayerHealth(playerid, 0.0);
		return 1;
	}

	if(PlayerInfo[playerid][pSTUNT] >= 1 || PlayerInfo[playerid][pINFERNUS1] >= 1 || PlayerInfo[playerid][pINFERNUS2] >= 1 || PlayerInfo[playerid][pINFERNUS3] >= 1 || PlayerInfo[playerid][pSTUNT2] >= 1 || PlayerInfo[playerid][pSTUNT3] >= 1 || PlayerInfo[playerid][pSTUNT4] >= 1 || PlayerInfo[playerid][pSTUNT5] >= 1 || PlayerInfo[playerid][pSTUNT5] >= 6 || PlayerInfo[playerid][pSTUNT6] >= 1 || PlayerInfo[playerid][pFUN] >= 1 || PlayerInfo[playerid][pInPlaneStunt] == 1)
	{
    	if(PlayerInfo[playerid][pVIP] == 0)
		{
 			PlayerInfo[playerid][pSTUNT] = 0;
