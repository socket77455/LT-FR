public OnPlayerUpdate(playerid)
{
	WeaponsAntiCheat(playerid);
	Speedometer(playerid);
	
	if(GetPlayerVirtualWorld(playerid) == FUN_ZONOS_PASAULIS)
	{
		Speed(playerid);
		Brakes(playerid);
	}
	
	CheckPaintSpray(playerid);

    new drunknew;
    drunknew = GetPlayerDrunkLevel(playerid);

    if (drunknew < 100) { // go back up, keep cycling.
        SetPlayerDrunkLevel(playerid, 2000);
    } else {

        if (pDrunkLevelLast[playerid] != drunknew) {

            new wfps = pDrunkLevelLast[playerid] - drunknew;

            if ((wfps > 0) && (wfps < 200))
                pFPS[playerid] = wfps;

            pDrunkLevelLast[playerid] = drunknew;
        }

    }
	
	SetPVarInt ( playerid, "PlayerAFK", GetTickCount ( ) );
	
	//  Anti itroll.cs
	static
	    vehicleid,
    	currState,
    	lastState [ MAX_PLAYERS ];

	vehicleid = GetPlayerVehicleID ( playerid );
	currState = GetPlayerState ( playerid );

	if ( vehicleid )
	{
		static
	   	 	lastCar [ MAX_PLAYERS ];

		if ( vehicleid != lastCar [ playerid ] )
		{
      		lastCar [ playerid ] = vehicleid;
      		if ( currState == lastState [ playerid ] )
      		{
				new
				    string [ 128 ],
				    pName [ 24 ];
				if(GetPlayerVirtualWorld(playerid) == 0)
				{
				GetPlayerName ( playerid, pName, 24 );
				format ( string, 128, "{FF0000}[INFO] þaidëjas %s(%i) buvo iðmestas uþ turbût draudþiamø modø naudojimà.", pName, playerid );
				SendMessageToAdmins(string);
      		    Kick(playerid);
      		    }
			}
		}
	}
	if ( lastState [ playerid ] != currState ) lastState [ playerid ] = currState;
	
	return 1;
}
