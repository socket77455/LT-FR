COMMAND:unwarn(playerid, params[])
{
	if(PlayerInfo[playerid][pLogged] == 0) return NotLogged(playerid);
	if(Player[playerid][pAdmin] < 4) return OnlyAdmins(playerid);
	if(strlen(params) >= 32) return 0;
	
	new rsn[33];
	new giveplayerid;
	new giveplayer [ 24 ];
	
	if(sscanf(params, "is", giveplayerid, rsn))
	{
    	SendClientMessage(playerid, COLOR_INFO, "USAGE: /unwarn <playerid> <reason>");
		return true;
	}

	if(strlen(rsn) > 32)
		return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid size of reason (don't go above 32 characters)", "KLAIDA: Per ilgas prieþasties tekstas. (Max: 32 simboliai)");

    if(!IsPlayerConnected(giveplayerid))
        return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: Invalid player ID!", "KLAIDA: Þaidëjas neprisijungæs!");

	if(giveplayerid == playerid)
	    return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You are not able to unwarn yourself!", "KLAIDA: Nuimti áspëjimà sau negalima!");

	if(Player[giveplayerid][pAdmin] > Player[playerid][pAdmin])
	    return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You are not able to unwarn a admin with a higher level than you!", "KLAIDA: Jûs negalite nuimti áspëjimà aukðtesnio rango administratoriui!");

	if(!Player[giveplayerid][pWarn])
		return SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: This player isn't warned!", "KLAIDA: Þaidëjas nëra áspëtas!");

	GetPlayerName(playerid, PlayerName, 24);
	GetPlayerName(giveplayerid, giveplayer, 24);

	new msg_lt[128],msg_en[128];
	format(msg_en,128, "[INFO] %s(%i) has been unwarned. (reason: %s) (admin: %s)", giveplayer, giveplayerid, rsn, PlayerName);
	format(msg_lt,128, "[INFO] %s(%i) buvo nuimtas áspëjimas. (Prieþastis: %s) (Administratorius: %s)", giveplayer, giveplayerid, rsn, PlayerName);
	
	print ( msg_lt );

	foreach (Player, i )
	{
		switch (PlayerInfo[i][pKalba])
		{
			case 0: SendClientMessage(i,COLOR_ERROR, msg_en);
			case 1: SendClientMessage(i,COLOR_ERROR, msg_lt);
		}
	}

	Player[giveplayerid][pWarn]--;
	return true;
}
