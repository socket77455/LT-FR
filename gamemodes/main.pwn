forward OnPassChangeCheck(playerid, newpass[], currentpw[]);
public OnPassChangeCheck(playerid, newpass[], currentpw[])
{
	new 
		str[128];

	if(cache_num_rows() == 0)
	{
    	switch (PlayerInfo[playerid][pKalba])
        {
            case 0:
            {
	            format(str, 128, "ERROR: \"%s\" isn't your current password (the checks are case sensitive!)", currentpw);
	            SendClientMessage(playerid, COLOR_ERROR, str);
            }
            case 1:
            {
	            format(str, 128, "KLAIDA: \"%s\" nëra teisingas jûsø kodas (patikrinkite ar neesate ájungæ 'Caps Lock'.)", currentpw);
	            SendClientMessage(playerid, COLOR_ERROR, str);
            }
        }
		return true;
	}

	if(strcmp(newpass, currentpw, false) == 0)
	{
    	switch (PlayerInfo[playerid][pKalba])
        {
            case 0:
            {
	            format(str, 128, "ERROR: New password (\"%s\") can't be same as the old password!", newpass);
				SendClientMessage(playerid, COLOR_ERROR, str);
            }
            case 1:
            {
	            format(str, 128, "KLAIDA: Naujasis kodas (\"%s\") negali bûti toks pats kaip senasis!", newpass);
				SendClientMessage(playerid, COLOR_ERROR, str);
            }
        }
		return true;
	}

	if(strcmp(newpass, PlayerName, true) == 0)
	{
    	switch (PlayerInfo[playerid][pKalba])
        {
            case 0:
            {
	            format(str, 128, "ERROR: The password (\"%s\") contains (part) of your name, hence it is invalid!", newpass);
				SendClientMessage(playerid, COLOR_ERROR, str);
            }
            case 1:
            {
	            format(str, 128, "KLAIDA: Kodas (\"%s\") panaðus á jûsø nickà (tai yra nesaugu)", newpass);
				SendClientMessage(playerid, COLOR_ERROR, str);
            }
        }
    	return true;
	}
	
	//mysql_escape_string   ( newpass, newpass, g_SQL, 14 );
	//print("mysql_escape_string 3");
	
	//new newsalt[11];
	//randomString(newsalt,sizeof(newsalt));
	//printf("New Salt: %s",newsalt);
	
	new newsalt[17];
	// generating random string of characters which is 10 character long
	for(new i; i < 16; i++)
	{
		// storing random character in every slot of our salt array
		newsalt[i] = random(94) + 33;
	}
	newsalt[16] = 0;
	
	new MyHash[65];
	SHA256_PassHash(newpass, newsalt, MyHash, 65);
	
	mysql_format(g_SQL, query, 512, "UPDATE `accounts` SET `hash` = '%e', `salt` = '%e' WHERE `player_name` = '%s'", MyHash, newsalt, PlayerName);
	mysql_tquery(g_SQL, query);
	
	Player[playerid][Password] = MyHash;
	Player[playerid][Salt] = newsalt;

    switch (PlayerInfo[playerid][pKalba])
    {
        case 0:
        {
            format(str, 128, "INFO: Your account's (\"%s\") password has been changed (old password \"%s\", new password \"%s\")", PlayerName, currentpw, newpass);
			SendClientMessage(playerid, COLOR_INFO, str);
			PlayerInfo[playerid][pApsauga] = 1;
			SetTimerEx("FloodApsauga",5000,0,"i", playerid);
        }
        case 1:
        {
            format(str, 128, "INFO: Jûsø saskaitos (\"%s\") kodas buvo pakeistas (senasis kodas \"%s\", naujasis kodas \"%s\")", PlayerName, currentpw, newpass);
			SendClientMessage(playerid, COLOR_INFO, str);
			PlayerInfo[playerid][pApsauga] = 1;
   			SetTimerEx("FloodApsauga",5000,0,"i", playerid);
        }
    }
    return 1;
}

