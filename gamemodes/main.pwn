forward PVPgo(playerid);
public PVPgo(playerid)
{
	GameTextForPlayer(playerid, "~r~FIGHT", 1000, 5);
	GameTextForPlayer(PlayerInfo[playerid][PVP], "~r~FIGHT", 1000, 5);
	TogglePlayerControllable(playerid, true);
	TogglePlayerControllable(PlayerInfo[playerid][PVP], true);
	
	PlayerPlaySound(playerid, 3200, 0.0, 0.0, 0.0);
	PlayerPlaySound(PlayerInfo[playerid][PVP], 3200, 0.0, 0.0, 0.0);
	
	new msg[128],pName[MAX_PLAYER_NAME],pName2[MAX_PLAYER_NAME];
