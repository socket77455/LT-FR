stock SendClientMessageLang(playerid, color, string1[], string2[])
{
	switch (PlayerInfo[playerid][pKalba])
	{
	    case 0: return SendClientMessage(playerid, color, string1);
	    case 1: return SendClientMessage(playerid, color, string2);
    }
	return 1;
}

stock SendClientMessageToAllLang(color, string1[], string2[])
{
	foreach(Player, i)
	{
		SendClientMessageLang(i, color, string1, string2);
	}
	return 1;
}

stock GameTextForPlayerLang(playerid, string1[], string2[], time, style)
{
	switch (PlayerInfo[playerid][pKalba])
	{
	    case 0: return GameTextForPlayer(playerid, string1, time, style);
	    case 1: return GameTextForPlayer(playerid, string2, time, style);
    }
	return 1;
}

stock getRandomPlayer()
{
    new
        count = 0,
        randomid = INVALID_PLAYER_ID
    ;
    while (randomid == INVALID_PLAYER_ID || !IsPlayerConnected(randomid)) 
    {
        count++;
        randomid = random(MAX_PLAYERS);
        if (count > 200 || IsPlayerConnected(randomid)) break;
    }
    return randomid;
}
