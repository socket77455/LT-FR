							format(ystring,sizeof(ystring),"~w~Laimejai ~g~$%d~n~~w~ir ~g~%d exp.~n~~w~vieta: ~g~%d",winrar, winrar/10,Ranking);
							GameTextForPlayer(playerid, ystring, 10000, 1);
	            		}
	            	}
				}

				PrizeMP--;
			}
			Ranking++;
			Participants--;
	        DisablePlayerRaceCheckpoint(playerid);
