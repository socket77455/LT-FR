stock FinishLogin(playerid)
{
	if  ( Player [ playerid ] [ HouseID ] != -1 )
	{
		//GetPlayerName   ( playerid, str, 24 );

		new
		    h = GetHouse ( Player [ playerid ] [ HouseID ] );

		if ( h == -1 )
		    Player [ playerid ] [ HouseID ] = -1;
		else
		    UpdateHouseDate ( playerid );
	}

    Player[playerid][pOnline] = 1;
	mysql_format(g_SQL, query, sizeof(query), "UPDATE `accounts` SET `online` = %d WHERE `user_id` = %d", Player[playerid][pOnline], Player[playerid][SQL_ID]);
