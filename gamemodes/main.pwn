					ShowPlayerDialog(playerid, DIALOG_LOGIN, DIALOG_STYLE_PASSWORD, "{FF0000}Prisijungimas", "Neteisingas slaptaþodis!\nPraðome ávesti teisingà slaptaþodá:", "Login", "");
				}
			}
		}
	}
	
	new veh = GetPlayerVehicleID(playerid);
	new engine,lights,alarm,doors,bonnet,boot,objective;

	if( dialogid == DIALOG_DM )
	{
		if( response )
		{
			switch( listitem )
			{
				case 0:
				{
					//if( DMA[ 0 ] >= 12 ) return SendClientMessage( playerid, COLOR_ERROR, "ERROR! This DM room is already full!" );
					PlayerInfo[playerid][pKova] = 1;
					NustatytiZaidejaBeTeamo(playerid);
					SetPlayerSpecialSkin(playerid);
					new msg_lt[128],msg_en[128],pName[MAX_PLAYER_NAME];
				    GetPlayerName(playerid, pName, MAX_PLAYER_NAME);
				    format(msg_en,128, "[/DM] %s(%i) has joined the First Deathmatch Arena.", pName, playerid);
				    format(msg_lt,128, "[/KOVA] %s(%i) prisijungë á pirmàjà kovos arenà.", pName, playerid);
				    
				    PlayerInfo[playerid][pECBUG] = 0;
				    PlayerInfo[playerid][pDMArena] = 1;
				    Kovos_zonos(playerid);
