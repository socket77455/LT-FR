GetHouse ( houseid )
{
	for ( new h = 0; h < MAX_HOUSES; h ++ )
	{
		if ( ( houseid == House [ h ] [ H_ID ] ) && HousePickup [ h ] )
		    return h;
	}
	return -1;
}

GetHouseCarID ( model )
{
	for ( new i; i < MAX_HOUSE_CARS; i ++ )
	{
	    if ( HouseCars [ i ] [ HC_Model ] == model )
	        return i;
	}
	return -1;
}

GetVehicleHouse ( vehicleid )
{
	for ( new h = 0; h < MAX_HOUSES; h ++ )
	{
	    if ( vehicleid == HouseCar [ h ] )
	        return h;
	}
	return -1;
}

