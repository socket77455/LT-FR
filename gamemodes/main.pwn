	print(msg_en);

	foreach (Player, i )
	{
		switch (PlayerInfo[i][pKalba])
		{
			case 0: SendClientMessage(i,COLOR_ERROR, msg_en);
			case 1: SendClientMessage(i,COLOR_ERROR, msg_lt);
		}
	}

	if(Player[giveplayerid][pWarn]==10)
	{
		Player[giveplayerid][pWarn]=5;
		
		new ipaddress[16];
		GetPlayerIp(giveplayerid,ipaddress,16);
		
