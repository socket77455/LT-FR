forward AtmutinaZaideja();
public AtmutinaZaideja()
{
    foreach (Player, i )
	{
		if(Player[i][pMute] > 0)
		{
  			if ( IsPlayerAFK ( i ) )
  				continue;
			Player[i][pMute]--;
		}

		if(Player[i][pMute]==1)
		{
			UnMutePlayer(i);
		}

		if(Player[i][pJail] > 0)
		{
  			if ( IsPlayerAFK ( i ) )
  				continue;
			Player[i][pJail]--;
		}
		
		if(Player[i][pJail]==1)
		{
			UnJailPlayer(i);
		}
		
		if(Player[i][pTeleAbuse] > 0)
		{
  			if ( IsPlayerAFK ( i ) )
  				continue;
			Player[i][pTeleAbuse]--;
		}
		
		if(Player[i][pTeleAbuse]==1)
		{
			AllowPlayerToTeleport(i);
		}
	}
	return 1;
}
