			return 1;
		}
		GivePlayerPCash(playerid,-500);
    	Minigamer_{ playerid } = true;
 		Iter_Add(_Minigamer, playerid );
		ResetPlayerWeapons(playerid);
		SetPlayerInterior(playerid, 0);
		SetPlayerPos(playerid, -316.5617,1841.0127,42.3594);
		SetPlayerVirtualWorld(playerid, WET_PASAULIS);
		JoinedWet[playerid] = 1;

	    SetPlayerColor(playerid, playerColors[playerid]);
	    
	    SetPlayerTeam(playerid, Team_Wet);
	    DisableSpawnProtection(playerid);
	    SetPlayerArmour(playerid, 0);

		SetPlayerSpecialSkin(playerid);
		
		new msg_lt[128],msg_en[128],pName[MAX_PLAYER_NAME];
	    GetPlayerName(playerid, pName, MAX_PLAYER_NAME);
	    format(msg_en,128, "[/WET] %s(%i) has joined the Wet Minigame.", pName, playerid);
	    format(msg_lt,128, "[/WET] %s(%i) prisijungë á Wet Mini þaidimà.", pName, playerid);

