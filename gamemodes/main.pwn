			Objects_[1][i] = CreateDynamicObject( 1649, gCoords[i][0], gCoords[i][1], gCoords[i][2], -270.000000, 0.000000, 0.000000, WET_PASAULIS, 0, -1, 150.0 );
			Iter_Add(_Objects, i );
	    }
	    inProgress = 1;
    }
    else
