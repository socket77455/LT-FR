COMMAND:colormatch(playerid, params[])
{
    if(Player[playerid][pJail] >= 1)
		return YouAreInJail(playerid);
    if(Player[playerid][pTeleAbuse] >= 1)
		return BannedTeleport(playerid);
	else if(PlayerInfo[playerid][pKova] == 1 || PlayerInfo[playerid][pRKova] == 1 || PlayerInfo[playerid][pMinigun] == 1 || PlayerInfo[playerid][pDisco] == 1 ||
		PlayerInfo[playerid][pDerby] == 1 || JoinedHay[playerid] == 1 || JoinedWet[playerid] == 1 || PlayerInfo[playerid][pSKY] == 1 || PlayerInfo[playerid][pGunGame] == 1 ||
		PlayerInfo[playerid][pFUN] == 1 || PlayerInfo[playerid][pInPlaneStunt] == 1 ||
		PlayerInfo[playerid][pSTUNT] >= 1 || PlayerInfo[playerid][pSTUNT2] >= 1 || PlayerInfo[playerid][pINFERNUS1] >= 1 || PlayerInfo[playerid][pINFERNUS2] >= 1 || PlayerInfo[playerid][pINFERNUS3] >= 1 ||
		PlayerInfo[playerid][pSTUNT3] >= 1 || PlayerInfo[playerid][pCBUG] == 1 || PlayerInfo[playerid][pDeliveryCar] == 1 || PlayerInfo[playerid][pBriefcase] == 1 ||
		RaceParticipant[playerid]>0 || dragParticipant[playerid]>0 ||
		MaratonasParticipant[playerid]>0 || PlayerInfo[playerid][PVPstate] > 1 && PlayerInfo[playerid][PVPstate] < 4 ||
		PlayerInfo[playerid][TDMstate] > 0 || PlayerInfo[playerid][pFuristas] == 1 || PlayerInfo[playerid][pInBMXgame] == 1)
			return UsePlayer(playerid);
	else if(PlayerInfo[playerid][pSTUNT4] >= 1)
		return UsePlayer(playerid);
	else if(PlayerInfo[playerid][pSTUNT5] >= 1)
		return UsePlayer(playerid);
    else if (PlayerInfo[playerid][pSTUNT6] >= 1)
		return UsePlayer(playerid);
	else if(PlayerInfo[playerid][pTeleporting]==1)
		return AlreadyTeleporting(playerid);

	if(IsPlayerInWarGames[playerid] == true) return UsePlayer(playerid);
	if(ZaidejasSurvival[playerid] == true) return UsePlayer(playerid);
	

	
	if(InCMEvent[playerid]) return SendClientMessage(playerid, COLOR_ERROR, "ERROR: You're already in the ColorMatch event.");
	if(ColorMatchStage == STAGE_PLAYING) return SendClientMessage(playerid, COLOR_ERROR, "ERROR: You can't join to the ColorMatch event because it started.");
	
 	if(GetPlayerPCash(playerid) < 500)
 	{
 	    SendClientMessageLang(playerid, COLOR_ERROR, "ERROR: You don't have enough cash! You need 500$!", "KLAIDA: Neturite pakankamai pinigø! Reikia 500$!");
		return 1;
	}
	GivePlayerPCash(playerid,-500);
	
	if(ColorMatchStage == STAGE_NONE) {
	    // init game
	    ColorMatchStage = STAGE_SETUP;
	    ColorMatchPlayers = 1;
	    ColorMatchTimer = SetTimerEx("CM_SetupTimer", 1000, false, "i", CM_SETUP_TIME);
	    ColorMatchFallTimer = ColorMatchEventTimer = -1;
	    
		ColorMatch_Arena();

		new msg_lt[128],msg_en[128],pName[MAX_PLAYER_NAME];
	    GetPlayerName(playerid, pName, MAX_PLAYER_NAME);
	    format(msg_en,128, "[/COLORMATCH] %s(%i) has started the ColorMatch Minigame, it will start in 60 seconds! Join Fee: 500$", pName, playerid);
	    format(msg_lt,128, "[/COLORMATCH] %s(%i) pradëjo ColorMatch Mini þaidimà, jis prasidës uþ 60 sekundþiø! Prisijungimo kaina: 500$", pName, playerid);
		foreach(Player, i)
		{
			switch (PlayerInfo[i][pKalba])
			{
				case 0: SendClientMessage(i,COLOR_YELLOW, msg_en);
				case 1: SendClientMessage(i,COLOR_YELLOW, msg_lt);
			}
		}
	}else{
	    // player join
	    if(ColorMatchPlayers >= CM_MAX_PLAYERS) return SendClientMessage(playerid, COLOR_ERROR, "ERROR: You can't join to the ColorMatch event because its full.");
	    ColorMatchPlayers++;

		new msg_lt[128],msg_en[128],pName[MAX_PLAYER_NAME];
	    GetPlayerName(playerid, pName, MAX_PLAYER_NAME);
	    format(msg_en,128, "[/COLORMATCH] %s(%i) has joined the ColorMatch Minigame.", pName, playerid);
	    format(msg_lt,128, "[/COLORMATCH] %s(%i) prisijungë á ColorMatch mini þaidimà.", pName, playerid);
	    
		foreach(Player, i)
		{
			switch (PlayerInfo[i][pKalba])
			{
				case 0: SendClientMessage(i,COLOR_INFO, msg_en);
				case 1: SendClientMessage(i,COLOR_INFO, msg_lt);
			}
		}
	}

    InCMEvent[playerid] = true;

	new randomPos = random(sizeof CM_Coords);
	SetPlayerPos(playerid, CM_Coords[randomPos][CM_CoordsX], CM_Coords[randomPos][CM_CoordsY], CM_Coords[randomPos][CM_CoordsZ]);
	SetPlayerVirtualWorld(playerid, CM_PASAULIS);
	SetCameraBehindPlayer(playerid);
	SetPlayerTeam(playerid, Team_CM);
	ResetPlayerWeapons(playerid);
	SetPlayerSpecialSkin(playerid);
	SetPlayerColor(playerid, playerColors[playerid]);
	return 1;
}
